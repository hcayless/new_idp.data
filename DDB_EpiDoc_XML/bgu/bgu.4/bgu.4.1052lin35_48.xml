         <head n="58091" xml:lang="en">
            <date>13 BC</date>
            <placeName>Alexandria</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="2" type="textpart"><ab>

    <lb n="35"/>Πρωτάρχωι 

    <lb n="36"/>παρὰ Πτολεμαίου τοῦ Πτολεμαίου Μακε

    <lb n="37" break="no"/>δόν<supplied reason="lost">ο</supplied>ς καὶ παρὰ Ἀπολλωνίου τοῦ Ἀγαθ<supplied reason="lost">ο</supplied>κλέ

    <lb n="38" break="no"/><supplied reason="lost">ο</supplied>υς Πέρσου τῆς ἐπιγονῆς καὶ τὴς τούτου 

    <lb n="39"/>γυναικὸς Ἀλεξάνδρας τῆς Διονυσίου <choice><reg>Περ

    <lb n="40" break="no"/>σίνης</reg><orig>Περ<lb n="40" break="no"/>σείνη<supplied reason="lost">ς</supplied></orig></choice> μετὰ κυρίου τοῦ ἀνδρός. συνχωροῦ

    <lb n="41" break="no"/><unclear>σιν</unclear> Ἀπολλώνιος καὶ Ἀλεξάνδρα <choice><reg>ἔχειν</reg><orig>ἔχιν</orig></choice> παρὰ τοῦ Πτο

    <lb n="42" break="no"/>λεμαίου <choice><reg>δάνειον</reg><orig>δάνηον</orig></choice> διὰ <choice><reg>χειρὸς</reg><orig>χιρὸς</orig></choice> ἐξ οἴκου ἀργυρίου Πτο

    <lb n="43" break="no"/>λεμαικοῦ <expan><ex>δραχμὰς</ex></expan> <num value="100">ρ</num> τόκων διδράχμων τοῦ μηνὸς ἑ

    <lb n="44" break="no"/>κάστου ὃ καὶ ἀποδώσειν ε<gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> τρι<unclear>ακα</unclear><gap reason="illegible" quantity="3" unit="character"/> 

    <lb n="45"/><gap reason="lost" quantity="6" unit="character"/><supplied reason="lost"> τοῦ</supplied> ἐνεστῶτος <num value="17">ιζ</num> <supplied reason="lost"><expan><ex>ἔτους</ex></expan> Καίσαρος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="46"/><gap reason="lost" quantity="9" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><unclear>ον</unclear> καταλα<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="47"/><gap reason="lost" quantity="10" unit="character"/><supplied reason="lost"> ἐ</supplied>ν τῷ ἐσχά<supplied reason="lost">τῳ μηνὶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="48"/><gap reason="lost" quantity="11" unit="character"/> καὶ αὐτὸν <gap reason="lost" extent="unknown" unit="character"/> 

