<ab>
    <lb n="1" rend="outdent"/>τῶι ἐπὶ τοῦ κριτηρίου 

    <lb n="2"/>παρὰ Αἰσώπου τοῦ Ἔρωτος <supplied reason="lost">Καί</supplied><unclear>σ</unclear>αρ<supplied reason="lost">ος</supplied> καὶ παρὰ <gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>Ἀρ<unclear>σινόης</unclear> <app type="editorial"><lem resp="P. van Minnen, Tyche 36 (2021) 198 (KorrTyche 1050)"><supplied reason="lost">τῆς</supplied> <unclear>Εὐδαίμονος</unclear> <unclear>μετὰ</unclear> <unclear>κυ</unclear>ρ<unclear>ί</unclear>ου τοῦ <supplied reason="omitted">ἀνδρὸς</supplied></lem><rdg><supplied reason="lost">μετ</supplied><unclear>ὰ</unclear> <unclear>κυρίου</unclear> <gap reason="illegible" quantity="3" unit="character"/><unclear>τουμω</unclear>ρου τοῦ</rdg></app> Τρύφω<unclear>νος</unclear> <gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">μεμι</supplied>

    <lb n="4" break="no"/><unclear>σθωκέναι</unclear> <unclear>Τρύφωνι</unclear> <gap reason="illegible" quantity="11" unit="character"/><unclear>λ</unclear>ει Ἔ<unclear>ρως</unclear> <unclear>τένα</unclear><supplied reason="lost">γος </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐπὶ τῆς κατὰ</supplied> 

    <lb n="5"/>Κανωβὸν <unclear>ταινίας</unclear> <gap reason="illegible" quantity="4" unit="character"/><unclear>ρ</unclear><gap reason="illegible" quantity="4" unit="character"/><unclear>ρν</unclear>υν <unclear>κῆπον</unclear> <gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>ἀπὸ Θῶυθ ἕως Μεσορὴ φόρου τοῦ ἑσταμένου <unclear>τῶν</unclear> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>κοσίων εἴκοσι, ἃς καὶ διορθούμενοι ἀνυπολόγους π<unclear>α</unclear><supplied reason="lost">ντὸς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>ἰσομερέσι <num value="3">τρισὶν</num> <unclear>διὰ</unclear> τετραμήνου ἐ<unclear>πὶ</unclear> <unclear>το</unclear>ῖς <unclear>υπ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>τῶι Αἰσώπῳ ἢ τοῖς παρὰ τοῦ Ἔρωτος, ἐὰν <unclear>δὲ</unclear> α<unclear>ἱ</unclear>ρῆ<supplied reason="lost">ται </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>ἀπὸ φόρων τοῦ τε <num value="6">ϛ</num> καὶ <num value="7">ζ</num> <expan><ex>ἔτους</ex></expan> Καίσαρος ἀργ<unclear>υρ</unclear><supplied reason="lost">ίου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>τῶν ὄντων ἐν τῶι κήπωι τὰ κράτιστα καὶ βέλτιστα <unclear>καὶ</unclear> <unclear>ο</unclear><supplied reason="lost">ὔσης </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>κράμβης καυλοὺς <num value="800">ω</num> κεφαλώνων λαμπάδας <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>θρίδακας κηπουριακὰς <num value="150">ρν</num> μηκω<unclear>ν</unclear>ίδας <num value="150">ρν</num> ε<gap reason="illegible" quantity="2" unit="character"/><app type="editorial"><lem resp="BL 1.97"><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τα</supplied>

    <lb n="14" break="no"/>γματικὰς</lem><rdg><gap reason="lost" extent="unknown" unit="character"/> <num value="3">γ</num> μωτικὰς</rdg></app>  <num value="140">ρμ</num> σταφυλῆς βοτρύας <num value="80">ὀγδοήκοντα</num> πα<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><num value="80">ὀγδοήκοντα</num> <choice><reg>κοχλιδίων</reg><orig>κοχλειδίων</orig></choice> <choice><reg>χοίνικας</reg><orig>χοίνεικας</orig></choice>  <num value="8">η</num> <app type="editorial"><lem resp="BL 1.97">σικύους κα<gap reason="lost" extent="unknown" unit="character"/></lem><rdg>σικύου <unclear>ταγὰ</unclear><supplied reason="lost">ς</supplied> <gap reason="lost" extent="unknown" unit="character"/></rdg></app> <supplied reason="lost">θρυα</supplied>

