    <lb n="17"/>σεύτλου πυθμένας ἑκατὸν <app type="editorial"><lem resp="P. van Minnen, Tyche 36 (2021) 198 (KorrTyche 1050)"><num value="8"><choice><reg>τεσσαράκοντα</reg><orig>τεσαράκ<add place="above">ο<supplied reason="omitted">ν</supplied></add><unclear>τα</unclear></orig></choice> ο<supplied reason="lost" cert="low">κτώ </supplied></num> <gap reason="lost" extent="unknown" unit="character"/></lem><rdg resp="BL 1.97"><num value="40"><choice><reg>τεσσαράκο<supplied reason="lost">ν</supplied>τα</reg><orig>τεσαράκ<add place="above">ο</add><supplied reason="lost">ν</supplied><unclear>τα</unclear></orig></choice></num> <unclear>ο</unclear><gap reason="lost" extent="unknown" unit="character"/></rdg><rdg><choice><reg>τέσσαρας</reg><orig>τεσαρα</orig></choice> κ<unclear>α</unclear>λο<gap reason="lost" extent="unknown" unit="character"/></rdg></app> <supplied reason="lost">εὐ</supplied>

    <lb n="18" break="no"/>τακτοῦντας ἀποφέρεσθαι τὰ ἐκ τῆς μισθώσ<supplied reason="lost">εως</supplied> <unclear>πε</unclear><supplied reason="lost">ριγινόμενα </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="19"/>κοπῇ, τὰ δὲ ἐκ τῆς μισθώσεως φορτία πάντα <gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/>χλωρᾶς τελώνιον καὶ τάξονται τὰ παρʼ αὐτ<supplied reason="lost">ῶ</supplied><unclear>ν</unclear> <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> κατὰ τὰ</supplied> 

    <lb n="21"/>προστάγματα καὶ διαγράμματα καὶ τοὺς ἐξ ἀρχῆς <unclear>ἐθ</unclear><supplied reason="lost">ισμοὺς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="22"/>περὶ τούτων αἰτίαις καὶ ζημίαις, εἴληφ<unclear>α</unclear>ν <unclear>δὲ</unclear> <unclear>Τρύφ</unclear><supplied reason="lost">ων </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="23"/>ἄτοκον διὰ τῆς Κάστορος κολλυβιστικῆς τραπέζης <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="24"/>Διονυσίου τραπέζης <expan>ἀργυρ<ex>ίου</ex></expan> <expan><ex>δραχμὰς</ex></expan> <num value="2500">Βφ</num>, ἅσπερ τυγχάν<supplied reason="lost">ουσιν </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐσχη</supplied>

    <lb n="25" break="no"/>κότες παρὰ Ἔρωτος τοῦ Καίσαρος διά τε τῶν αὐτ<unclear>ῶ</unclear><supplied reason="lost">ν τραπεζῶν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="26"/>ἐπιτελεῖν οὗ μεμίσθωνται κήπου τὰ καθήκον<unclear>τ</unclear><supplied reason="lost">α </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="27"/>μηδὲ παραλείποντας ἔργον μηδὲν τῶν εἰς εὐεργ<supplied reason="lost">ίαν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="28"/>κατὰ καιρὸν <choice><reg>λαχανείᾳ</reg><orig>λαχανήᾳ</orig></choice> καὶ παρεχόμενοι σύνφυτον κ<supplied reason="lost">αὶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="29"/>κατὰ τρόπον ἐν τοῖς δέουσι καιροῖς καὶ τῶν δὲ ὄντων <unclear>δέ</unclear><supplied reason="lost">νδρων </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="30"/>εἰς τὸ ζωφυτεῖν καὶ εὐθηνεῖν μηδὲν ἐκκόπτοντες <unclear>λ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="31"/>ἐκ τοῦ ἰδίου <choice><reg>τὰ αὐτὰ</reg><orig>ταυτὰ</orig></choice> γένηι αὐτενίαυτα οὐδενὶ χείρον<supplied reason="lost">α </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="32"/>ποτισμοὺς μὴ ἐξόντος αὐτοῖς ἐγλιπεῖν τὴν μίσθω<supplied reason="lost">σιν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="33"/>παραδοῦναι αὐτοὺς τὸν κῆπον σύνφυτον καὶ ζωφυτ<supplied reason="lost">οῦντα </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> βε</supplied>

    <lb n="34" break="no"/>βλαμμένον καὶ ἐν α<unclear>ὐ</unclear>τῶι <choice><reg>συντίμησιν</reg><orig>συντείμησιν</orig></choice> <choice><reg>λαχανείας</reg><orig>λαχανήας</orig></choice> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="35"/><unclear>καὶ</unclear> τῶν ἄλλων τῶν εἰθισμένων <choice><reg>συντιμᾶσθαι</reg><orig>συντειμᾶσθαι</orig></choice> πλὴν π<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="36"/>ἐὰ<unclear>ν</unclear> <unclear>δὲ</unclear> πλ<unclear>εί</unclear>ω παραδῶσι, προσαποδώσει αὐτοῖς ὁ Αἴσω<supplied reason="lost">πος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="37"/>τὸ <unclear>λο</unclear>ιπὸν παραχρῆμα, ἐὰν δὲ ἐλάττονος <unclear>γίνητα</unclear><supplied reason="lost">ι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="38"/><choice><reg>συντίμησιν</reg><orig>συντείμησιν</orig></choice> προσαποδώσουσιν καὶ αὐτοὶ τῶι Αἰσ<supplied reason="lost">ώπωι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="39"/><unclear>το</unclear>ντας δὲ τοὺς μεμισθωμένους ἐκ τοῦ κήπο<supplied reason="lost">υ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="40"/><unclear>ἢ</unclear> πείθεσθαι περὶ τῆς τούτ<unclear>ων</unclear> ἀξίας ὑπὸ τοῦ Αἰσώπ<supplied reason="lost">ου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="41"/>ἐὰν δέ τι παραβαίνωσιν οἱ μεμισθωμένοι, εἶναι αὐ<supplied reason="lost">τοὺς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="42"/>πάντα ἐπιτελέσαι καὶ ἐκτεῖσαι τῶι Αἰσώπωι τάς τε πρ<supplied reason="lost">οκειμένας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="43"/><expan>δραχμ<ex>ὰς</ex></expan> <num value="2451">Βυνα</num> καὶ τὰς τῆς προχρήσεως <expan>ἀργυρί<ex>ου</ex></expan> <expan><ex>δραχμὰς</ex></expan> <num value="2500"><unclear>Β</unclear><supplied reason="lost" cert="low">φ</supplied></num><supplied reason="lost" cert="low"> <app type="editorial"><lem resp="BL 1.97">καὶ τόκους διδράχμους </lem><rdg><gap reason="illegible" extent="unknown" unit="character"/></rdg></app> καὶ οὓς ἐὰν ἐνοφει</supplied>

    <lb n="44" break="no"/>λήσωσι πρὸς τὸ ἐνεστὸς <num value="8">η</num> <expan><ex>ἔτος</ex></expan> φόρους, ὡσαύτω<supplied reason="lost">ς δὲ καὶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="45"/>ἐξαιρέτων τὴν πρὸς τὸν γνώμονα <choice><reg>τιμὴν</reg><orig>τειμὴν</orig></choice> καὶ τὰ βλάβη <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="46"/>ἐξεῖναι τῶι Αἰσώπωι παρασυνγραφοῦντας αὐτοὺς μ<supplied reason="lost">εθισταμένωι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="47"/>καὶ ὁμοίως ἀναπράσσειν τὸ ἐσόμενον ἀφεύρεμα κ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="48"/>τῆς πράξεως γεινομένης ἐκ τῶν <num value="2">δύο</num> ὄντων ἀλληλεγγύων εἰς <supplied reason="lost">ἔκτισιν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="49"/>καὶ ἐκ τῶν <choice><reg><supplied reason="omitted">ὑπ</supplied>αρχόντων</reg><orig>ἁρχόντων</orig></choice> αὐτοῖς πάντων καθάπερ <choice><reg>ἐκ</reg><orig>ἐγ</orig></choice> δίκης καὶ μὴ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="50"/>δʼ αὐτῶν ἐπιτελούντων καὶ τὸν Αἴσωπον βεβαι<unclear>οῦν</unclear> αὐτοῖς <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="51"/>ἀφαιρεῖσθαι ἐντὸς τούτου, παρέχεσθαι δʼ αὐτοῖς ἐπὶ το<gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἢ καὶ αὐτὸς τῷ ἴσῳ ἐπιτίμῳ</supplied> 

    <lb n="52"/>ἔνοχος ἔστω. <expan>ἀξιοῦ<ex>μεν</ex></expan>. 
<milestone rend="paragraphos" unit="undefined"/>
    <lb n="53"/><expan><ex>ἔτους</ex></expan> <num value="8">η</num> Καίσαρος Φαμενὼθ <num value="18">ιη</num>. 
