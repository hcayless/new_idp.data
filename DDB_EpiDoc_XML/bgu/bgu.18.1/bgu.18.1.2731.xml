         <head n="69805" xml:lang="en">
            <date>87/6 BC</date>
            <placeName>Herakleopolis</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve">
<ab>
    <lb n="1"/>Πετειμούθηι βασι<unclear>λικ</unclear><supplied reason="lost">ῷ γραμμ</supplied><unclear>ατε</unclear>ῖ

    <lb n="2"/>παρὰ Ἡρακλείας τῆς Διο<unclear>ν</unclear><supplied reason="lost">υσ</supplied><unclear>ί</unclear>ου τῶν ἐξ Ἡρα<unclear>κ</unclear><supplied reason="lost">λέους</supplied>

    <lb n="3"/>πόλεως· ἐν τῶι <num value="31">λα</num> <expan><ex>ἔτει</ex></expan> δι<unclear>α</unclear>γεγραφηκυῖα <supplied reason="lost">ἐπὶ τὴν</supplied>

    <lb n="4"/>ἐν τῆι αὐτῆι πόλει βασιλικ<unclear>ὴ</unclear>ν τράπ<unclear>εζ</unclear>αν <supplied reason="lost">ὑπὲρ</supplied>

    <lb n="5"/>τῶν <app type="editorial"><lem resp="BL 12.29"><choice><reg>ἀναγραφεισῶν</reg><orig>ἀναγραφέντων</orig></choice></lem><rdg>ἀναγραφέντων</rdg></app> οἰκιῶν Ἀπολλωνί<unclear>ο</unclear><supplied reason="lost">υ</supplied>

    <lb n="6"/>τοῦ Νικάνδρου τοῦ πραγματευομένου

    <lb n="7"/>μετὰ μετόχων τὴν <choice><reg>ζυτηράν</reg><orig>ζυτηρηάν</orig></choice>, ὧν <unclear>τ</unclear><supplied reason="lost">ά τε</supplied>

    <lb n="8"/>μέτρα καὶ <unclear>α</unclear>ἱ <choice><reg>γειτνίαι</reg><orig>γιτνίαι</orig></choice> <choice><reg>ὑπόκεινται</reg><orig>ὑπόκειται</orig></choice>, <expan>χα<ex>λκοῦ</ex></expan> <expan><ex>τάλαντα</ex></expan> <num value="21">κα</num> <app type="editorial"><lem resp="BL 12.29"><unclear>ε</unclear><supplied reason="lost">ἰς τό</supplied></lem><rdg><unclear>ἀ</unclear><supplied reason="lost">ξιῶ</supplied></rdg></app>

    <lb n="9"/><choice><reg>ἐκδοθῆναί</reg><orig><unclear>ἐ</unclear>γδοθῆναί</orig></choice> μοι τὴν καθήκουσαν διαγραφ<unclear>ὴ</unclear><supplied reason="lost" cert="low">ν ὑπὲρ </supplied>

    <lb n="10"/><gap reason="illegible" quantity="2" unit="character"/> <unclear>τ</unclear>οῦ χαλκοῦ ἀναφερομένου διὰ τῶν <app type="editorial"><lem resp="Cf. BL 12.29"><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="4" unit="character" precision="low"/>

    <lb n="11"/><gap reason="illegible" quantity="1" unit="character"/><unclear>ῶ</unclear>ν</lem><rdg><unclear>μ</unclear><supplied reason="lost">ετόχων ὑ</supplied><lb n="11" break="no"/><unclear>μῶ</unclear>ν</rdg></app>· <choice><reg>προαιρουμένη</reg><orig>προαιρουμένηι</orig></choice> δὲ <unclear>π</unclear>ροσδιαγράψασα <supplied reason="lost" cert="low">αὐτοῖς </supplied>

    <lb n="12"/><unclear>ἄλλα</unclear>ς <expan>χα<ex>λκοῦ</ex></expan> <expan><ex>δραχμὰς</ex></expan> <num value="3000">Γ</num> <choice><reg>ἐκδοθῆναί</reg><orig>ἐγδοθῆναί</orig></choice> μοι τὴν διαγρ<supplied reason="lost">αφὴν</supplied>

    <lb n="13"/>ἀξιῶ ἐὰν φαίνηται συντάξαι τοῖς πρὸς τού<supplied reason="lost" cert="low">τοις </supplied>

    <lb n="14"/>οὖσι <choice><reg>ἐκδοῦναί</reg><orig>ἐγδοῦναί</orig></choice> μ<supplied reason="lost">ο</supplied>ι <supplied reason="lost">τ</supplied>ὴν διαγραφὴν ἵνʼ ὦ ἀν<supplied reason="lost">τειλημ</supplied>

    <lb n="15" break="no"/>μέ<unclear>ν</unclear>η· <app type="editorial"><lem resp="L. Ferretti, Pylon 5 (2024) 9 §18">εὐ<unclear>τ</unclear><supplied reason="lost">ύχει</supplied></lem><rdg><unclear>δ</unclear><supplied reason="lost">ι</supplied>ευ<unclear>τ</unclear><supplied reason="lost">ύχει·</supplied></rdg></app>

    <lb n="16"/>ἐν Ἡρα<unclear>κλ</unclear>έους πόλει ἐντὸς τοῦ Ἁκώριος <unclear>π</unclear><supplied reason="lost">ερι</supplied>
 
    <lb n="17" break="no"/>βόλου ο<supplied reason="lost">ἰ</supplied><unclear>κ</unclear>ίαν <del rend="erasure"><expan>βο<ex>ρρᾶ</ex></expan> ἐπὶ <expan>νό<ex>του</ex></expan></del> <expan>π<ex>ηχῶν</ex></expan> <num value="30">λ</num> <del rend="erasure"><add place="above">ἀπὸ</add> <unclear>β</unclear><add place="above"><expan>ο<ex>ρρᾶ</ex></expan></add></del> σ<unclear>ὺ</unclear>ν τῆι ἀπὸ νότου αὐ<unclear>λ</unclear><supplied reason="lost">ῇ</supplied>

    <lb n="18"/>ἐπὶ <expan>π<ex>ηχῶν</ex></expan> <gap reason="illegible" quantity="1" unit="character"/> <del rend="erasure"><unclear>πρὸ</unclear>ς ἀπηλιώτ<unclear>η</unclear>ν</del> <add place="above"><del rend="erasure"><gap reason="illegible" quantity="2" unit="character"/></del></add> <unclear>κ</unclear>αὶ προ<unclear>νή</unclear><supplied reason="lost">σιον</supplied>

    <lb n="19"/>ἀπὸ βορρᾶ, ὧν <choice><reg>γείτονες</reg><orig>γίτονες</orig></choice> νότου τόποι <gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" quantity="6" unit="character"/><supplied reason="lost">,</supplied>

    <lb n="20"/>βο<unclear>ρ</unclear>ρᾶ καὶ λιβὸς ῥύμη, ἀπηλιώτου <unclear>Ν</unclear><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="6" unit="character"/>

    <lb n="21"/><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="6" unit="character"/>
<milestone rend="paragraphos" unit="undefined"/>
    <lb n="22"/>πρὸς τῆι <choice><reg>πλατείᾳ</reg><orig><unclear>π</unclear>λα<unclear>τ</unclear>ίᾳ</orig></choice> <supplied reason="omitted">ὁδῷ</supplied> τ<unclear>ῆ</unclear>ς ἱε<unclear>ρ</unclear><supplied reason="lost">ᾶ</supplied><unclear>ς</unclear> πύλ<unclear>ης</unclear> μέρο<supplied reason="lost">ς </supplied><gap reason="lost" quantity="6" unit="character"/>

    <lb n="24"/>οἰκίας κατεσπ<unclear>ασμ</unclear>ένης <expan>π<ex>ηχῶν</ex></expan> <num value="8">η</num> ἐπὶ <expan>π<ex>ηχῶν</ex></expan> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="6" unit="character"/>

    <lb n="25"/>ἀπὸ βορρᾶ, ὧν <choice><reg>γείτονες</reg><orig>γί<unclear>τ</unclear>ονες</orig></choice> νότου Σωτηρί<unclear>χ</unclear><supplied reason="lost">ου</supplied>

    <lb n="26"/><supplied reason="lost">το</supplied>ῦ <gap reason="illegible" quantity="3" unit="character"/>ου, <unclear>βορρᾶ</unclear> ῥύμη, λιβὸς <unclear>ισ</unclear>ο<gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="6" unit="character"/><supplied reason="lost">,</supplied>

    <lb n="27"/><supplied reason="lost">ἀπ</supplied><unclear>η</unclear><supplied reason="lost">λ</supplied>ιώ<unclear>τ</unclear>ου <unclear>Π</unclear><gap reason="illegible" quantity="1" unit="character"/>ρρα<unclear>τ</unclear><supplied reason="lost">ο</supplied>ς ο<supplied reason="lost">ἰ</supplied>κία· <expan><ex>τάλαντα</ex></expan> <supplied reason="lost" cert="low"><expan>χα<ex>λκοῦ</ex></expan> <num value="21">κα</num> </supplied>
