         <head n="23529" xml:lang="en">
            <date>AD 265</date>
            <placeName>Hermoupolis Magna</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="4" type="textpart"><ab>

    <lb n="91"/>Φλαυ<hi rend="diaeresis">ί</hi>ῳ Π<supplied reason="lost">α</supplied>νίσκῳ τῷ καὶ Λόγγῳ στρατηγ<supplied reason="lost">ῷ</supplied> 

    <lb n="92"/><expan>Ἑρμοπολ<ex>ίτου</ex></expan> 

    <lb n="93"/>παρὰ Αὐρη<supplied reason="lost">λ</supplied>ίων Ἀχιλλέως Πετοβάσθιος <supplied reason="lost">μη</supplied>

    <lb n="94" break="no"/>τρὸς Εὖτος ὡς <expan><ex>ἐτῶν</ex></expan> <num value="48">μη</num> οὐλὴ γόνατι ἀ<supplied reason="lost">ριστερῷ</supplied> 

    <lb n="95"/>καὶ Διονυσίου Σαραπίωνος ἀμφοτ<supplied reason="lost">έρων</supplied> 

    <lb n="96"/>κωμαρχῶν κώμης Σινκατ<supplied reason="lost">α</supplied>πὴ δ<supplied reason="lost">ιʼ ἐμοῦ</supplied> 

    <lb n="97"/><unclear>τ</unclear>οῦ <unclear>παρ</unclear>όντος Ἀχιλλέως. δίδομεν καὶ <supplied reason="lost">προσ</supplied>

    <lb n="98" break="no"/>αγ<unclear>γέλλομ</unclear>εν τὸν ὑπογεγραμμένον <supplied reason="lost">ἐπὶ</supplied> 

    <lb n="99"/>μῆ<unclear>νας</unclear> ἓξ <num value="6"/> εἰς <choice><reg>δεσμοφυλακίαν</reg><orig>δεσμοφυλα<lb n="54" break="no"/>κείαν</orig></choice> <supplied reason="lost">τοῦ</supplied> 

    <lb n="100"/>ἐ<unclear>ν</unclear> <unclear>τῇ</unclear> <unclear>μητ</unclear>ροπόλει <choice><reg>δεσμοφυλακίου</reg><orig>δεσμοφυλακείου</orig></choice> 

    <lb n="101"/>ὄντ<unclear>α</unclear> <unclear>ε</unclear>ὔπορον καὶ ἐπιτήδειον κι<unclear>ν</unclear><supplied reason="lost">δύνῳ</supplied> 

    <lb n="102"/>ἡμῶν καὶ πάντων τῶν καταμενό<supplied reason="lost">ν</supplied>

    <lb n="103" break="no"/>των ἐν τῇ αὐτῃ κώμῃ ἐξ ἀλληλε<supplied reason="lost">γγύ</supplied>

    <lb n="104" break="no"/>ης <supplied reason="lost">ὃ</supplied>ν καὶ ἡμεῖς ἐγγυώμεθα. ἔστ<supplied reason="lost">ι δέ·</supplied> 

    <lb n="105"/>Κο<supplied reason="lost">λ</supplied>λ<supplied reason="lost">ο</supplied>ῦθος Κουρήνιος <expan>μητ<ex>ρὸς</ex></expan> Εὖτ<supplied reason="lost">ος</supplied> 

    <lb n="106"/>ὡς <expan><ex>ἐτῶν</ex></expan> <num value="40">μ</num> <expan>πόρ<ex>ον</ex></expan> <expan>ἔχ<ex>ων</ex></expan> <supplied reason="lost"><expan><ex>δραχμῶν</ex></expan> <num value="1000">Α</num>.</supplied> 

    <lb n="107"/>διευτύχει. 

    <lb n="108"/><expan><ex>ἔτους</ex></expan> <num value="12">ιβ</num> Αὐτοκράτορος Πο<supplied reason="lost">υπλίου</supplied> 

    <lb n="109"/>Λικιννίου Γαλλιηνοῦ Γερμανικοῦ <supplied reason="lost">μεγίστου</supplied> 

    <lb n="110"/><supplied reason="lost">Εὐσεβο</supplied>ῦς Εὐτυχοῦς Σεβαστοῦ Παῦν<supplied reason="lost">ι <num value="6">ϛ</num></supplied> 

    <lb n="111"/><supplied reason="lost">οἱ </supplied><expan><supplied reason="lost">προ</supplied>κ<ex>είμενοι</ex></expan> <expan>κωμά<ex>ρχαι</ex></expan> διʼ ἐμοῦ Ἀχιλλέως ἐ<supplied reason="lost">πιδεδώ</supplied>

    <lb n="112" break="no"/><supplied reason="lost">καμε</supplied>ν. <expan>Αὐρ<ex>ήλιος</ex></expan> Ἑρμησίων <expan>ἔγρα<ex>ψα</ex></expan> <expan>ὑπ<ex>ὲρ</ex></expan> <expan>αὐτ<ex>οῦ</ex></expan> <expan>γρά<ex>μματα</ex></expan> <unclear>μ</unclear><supplied reason="lost">ὴ<expan>εἰδ<ex>ότος</ex></expan>.</supplied> 

    <lb n="113"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> Παῦν<supplied reason="lost">ι <num value="6">ϛ</num> </supplied><gap reason="lost" extent="unknown" unit="character"/>  
