    <lb n="36" break="no"/><supplied reason="lost">καμεν. </supplied><expan><supplied reason="lost">Αὐ</supplied>ρήλ<ex>ιος</ex></expan> Ἑρμησίων <expan>ἔγρα<ex>ψα</ex></expan> ὑπὲρ <expan>αὐτ<ex>ῶν</ex></expan> <expan>γρά<ex>μματα</ex></expan> μὴ <expan>εἰδ<ex>ότων</ex></expan> 

    <lb n="37"/><supplied reason="lost"><expan>Αὐρ<ex>ήλιος</ex></expan> </supplied><supplied reason="lost" cert="low">Σερ</supplied><unclear>ῆ</unclear>νος <expan>ὑπη<unclear>ρ</unclear>έτ<ex>ης</ex></expan> <app type="editorial"><lem resp="BL 1.133">δημοσίᾳ</lem><rdg>δημόσι<unclear>ος</unclear></rdg></app> 

