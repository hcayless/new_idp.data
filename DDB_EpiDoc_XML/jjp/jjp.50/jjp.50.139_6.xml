<ab>
    <lb n="1"/><g type="rho-cross"/> ἐμέτρησεν <expan>κ<ex>αὶ</ex></expan> ἐνεβάλετ<unclear>ο</unclear> <unclear>ὑπὲρ</unclear> <expan>κώμ<ex>ης</ex></expan> Ἀφροδίτης <expan>δ<ex>ιὰ</ex></expan> τῶν <choice><reg>πρωτοκωμητῶν</reg><orig>πρωτοκομ<unclear>ητῶν</unclear></orig></choice> <unclear>τῆς</unclear> <supplied reason="lost">αὐτῆς κώμης</supplied>

    <lb n="2"/><expan>δ<ex>ιὰ</ex></expan> Βίκ<unclear>τορος</unclear> <unclear>πρεσβυτέρ</unclear><supplied reason="lost">ου <expan>κ<ex>αὶ</ex></expan> Ἀπολλῶτος <expan>κ<ex>αὶ</ex></expan> Σενουθίου <expan><ex>ὑπέρ</ex></expan> τε αὐτῶν κτήσεως</supplied> <unclear>καὶ</unclear> <expan><unclear>δι</unclear>αφόρ<ex>ων</ex></expan> <choice><reg>κτητόρων</reg><orig><expan>κτητόρο<ex>ν</ex></expan></orig></choice>

    <lb n="3"/>τῆς αὐτῆς <expan>κώμ<ex>ης</ex></expan> εἰς <expan>πλοῖ<ex>α</ex></expan> μοναστηρ<unclear>ίου</unclear> <choice><reg>Μετανοίας</reg><orig>Μετανίας</orig></choice> <expan>δι<ex>ὰ</ex></expan> τοῦ θεοσεβεστάτου <expan>ἀββ<ex>ᾶ</ex></expan>

    <lb n="4"/>Θεο<unclear>δ</unclear>οσί<unclear>ου</unclear> <choice><reg>προεστῶτος</reg><orig>προεστῶτων</orig></choice> <expan>δ<ex>ιὰ</ex></expan> τῶν <choice><reg>θεοσεβεστάτων</reg><orig>θεοσεββεστάτων</orig></choice> <expan>ἀββ<ex>ᾶ</ex></expan> Ἀναστασίου <expan>κ<ex>αὶ</ex></expan> <expan>ἀββ<ex>ᾶ</ex></expan> Ἰσχυρί

    <lb n="5" break="no"/>ωνος <expan>μοναζ<ex>όντων</ex></expan> <expan><unclear>κ</unclear><ex>αὶ</ex></expan> <expan><unclear>δ</unclear>ια<unclear>κ</unclear><ex>ονητῶν</ex></expan> <choice><reg>τῆς Ἀνταιοπολιτῶν <expan><ex>πόλεως</ex></expan></reg><orig>Ἀντ<unclear>α</unclear>ι<unclear>ου</unclear>πολ<unclear>ι</unclear>τῶ<unclear>ν</unclear></orig></choice> <expan><ex>ὑπὲρ</ex></expan> <expan>ἐ<unclear>μβ</unclear><ex>ολῆς</ex></expan> <unclear>τ</unclear>ῆς αὐτῆ<unclear>ς</unclear> <expan>κώμ<ex>ης</ex></expan> <choice><reg>κανόνος</reg><orig>κα<unclear>νό</unclear>νας</orig></choice> <num value="6">ἕκτης</num>

    <lb n="6"/><expan>ἰνδικ<ex>τίονος</ex></expan> σίτο<supplied reason="lost">υ</supplied> ἀρτάβας <num value="5759"><choice><reg>πεντακισχιλίας</reg><orig><unclear>πε</unclear>ντα<unclear>κ</unclear>εσχ<unclear>ιλ</unclear>ε<unclear>ίας</unclear></orig></choice> ἑ<unclear>π</unclear>τα<unclear>κ</unclear>οσίας πεντήκοντα ἐννέα</num>

    <lb n="7"/><expan>σὺ<ex>ν</ex></expan> <expan>να<ex>ύ</ex>λ<ex>οις</ex></expan> <expan>κ<ex>αὶ</ex></expan> <num value="1/100"><choice><reg>ἑκατοσταῖς</reg><orig>ἑκατεστῆς</orig></choice></num> <expan>κ<ex>αὶ</ex></expan> <num value="1/20"><choice><reg>εἰκοσταῖς</reg><orig>εἰκο<unclear>σ</unclear>τ<unclear>ε͂</unclear>ς</orig></choice></num>, <expan>γί<ex>νονται</ex></expan> <expan>σίτ<ex>ου</ex></expan> <expan><ex>ἀρτάβαι</ex></expan> <num value="5759">Εψνθ</num> <expan>μόν<ex>αι</ex></expan>. <expan>κ<ex>αὶ</ex></expan> εἰς <surplus><supplied reason="omitted">σ</supplied>ὴν</surplus> <unclear>ὑμ</unclear>ῶν ἀσφάλειαν

    <lb n="8"/><expan>κ<ex>αὶ</ex></expan> τοῦ <choice><reg>δημοσίου</reg><orig>τεμοσίου</orig></choice> <choice><reg>πεποίημαι</reg><orig>πεπ<unclear>οίμ</unclear>ε</orig></choice> τα<unclear>ύτ</unclear>ην <supplied reason="omitted">τὴν</supplied> <choice><reg>ἀπόδειξιν</reg><orig>ἀπότιξιν</orig></choice> <unclear>ὡ</unclear>ς <expan>πρόκ<ex>ειται</ex></expan>. <choice><reg>Ἀναστάσιος</reg><orig>Ἀναστασίου</orig></choice> <expan>κ<ex>αὶ</ex></expan> <expan>ἀ<unclear>ββ</unclear><ex>ᾶ</ex></expan> <choice><reg>Ἰσχυρίων</reg><orig><unclear>Ἰσ</unclear>χ<unclear>υρ</unclear>ίω<unclear>νο</unclear><supplied reason="lost">ς</supplied></orig></choice>

    <lb n="9"/><choice><reg>διακονηταὶ</reg><orig><expan><unclear>δ</unclear>ιακ<ex>ονητῶν</ex></expan></orig></choice> <expan>δ<ex>ιὰ</ex></expan> <unclear>ἐμοῦ</unclear> <unclear>Τιμοθέου</unclear> <choice><reg>γραμματέως</reg><orig><expan><unclear>γραματ</unclear><ex>έως</ex></expan></orig></choice> <unclear>τῆς</unclear> <unclear>αὐτῆς</unclear> <expan><unclear>κώμ</unclear><ex>ης</ex></expan> <choice><reg>πεποιήμεθα</reg><orig><unclear>πε</unclear>ποίμ<unclear>ε</unclear></orig></choice> τὴν <choice><reg>ἀπόδειξιν</reg><orig>ἀπότεκξιν</orig></choice>

    <lb n="10"/><choice><reg>τῶν</reg><orig><unclear>το͂</unclear>ν</orig></choice> τοῦ σίτου ἀρταβῶν <num value="5759"><choice><reg>πεντακισχιλίων</reg><orig>πε<unclear>ντ</unclear><supplied reason="lost">α</supplied>κεσχιλίας</orig></choice> <choice><reg>ἑπτακοσίων</reg><orig>ἑπτακοσίας</orig></choice> πεντήκοντα ἐννέα</num> ὡς <expan><unclear>π</unclear>ρ<unclear>ό</unclear>κ<ex>ειται</ex></expan>. <expan>ἐγράφ<ex>η</ex></expan> <unclear>Θω</unclear>θ <gap reason="illegible" atLeast="1" atMost="2" unit="character"/> <expan><unclear>ἰνδ</unclear><supplied reason="lost">ι</supplied><unclear>κ</unclear><ex>τίονος</ex></expan> <num value="6">ϛ</num>.

    <lb n="11"/><space quantity="1" unit="line"/>

    <lb n="12"/><g type="rho-cross"/> ἐμέτρησεν καὶ ἐνεβάλετο ὑπὲρ <expan>κώμ<ex>ης</ex></expan> <expan><unclear>Ἀ</unclear>φροδί<ex>της</ex></expan> <expan>δ<ex>ιὰ</ex></expan> τῆς <choice><reg><app type="alternative"><lem>θαυμασιότητος</lem><rdg>θαυμασιωτάτης <supplied reason="omitted">κοινότητος</supplied></rdg></app></reg><orig>θ<unclear>αυ</unclear>μασι<unclear>ο</unclear>τατης</orig></choice> τῶν <choice><reg>πρωτοκωμητῶν</reg><orig>προτοκομητῶν</orig></choice>

    <lb n="13"/><unclear>τῆ</unclear>ς <supplied reason="lost">αὐ</supplied><unclear>τ</unclear>ῆς <expan>κώμ<ex>ης</ex></expan>, <expan>δ<ex>ιὰ</ex></expan> Ἀπολλῶτος κ<supplied reason="lost">αὶ</supplied> <unclear>Βί</unclear>κ<unclear>το</unclear>ρος καὶ <choice><reg>Σενουθίου</reg><orig>Σινουθίου</orig></choice> <expan><ex>ὑπέρ</ex></expan> τε <choice><reg>αὐτῶν</reg><orig>αὐτο͂ν</orig></choice> κτή<unclear>σ</unclear>εως καὶ <choice><reg>διαφόρων</reg><orig><expan>διαφόρο<ex>ν</ex></expan></orig></choice>

    <lb n="14"/><choice><reg>κτητόρων</reg><orig>κτητόρον</orig></choice> τῆς αὐτῆς <expan>κώμ<ex>ης</ex></expan> εἰς <expan>πλοῖ<ex>α</ex></expan> τοῦ μοναστηρίου <choice><reg>Μετανοίας</reg><orig>Μετανίας</orig></choice> τῆς <choice><reg>μεγαλοπόλεως</reg><orig>μεγαλεπόλεως</orig></choice>

    <lb n="15"/><choice><reg>Ἀλεξανδρείας</reg><orig>Ἀλεξανδρίας</orig></choice>, <expan>δ<ex>ιὰ</ex></expan> τῶν θεοφιλέστατ<unclear>ων</unclear> <expan>ἀββ<ex>ᾶ</ex></expan> Ἀναστ<unclear>α</unclear>σί<unclear>ο</unclear>υ καὶ <unclear>Ἀναστ</unclear>ασίου <expan>μοναζ<ex>όντων</ex></expan> καὶ <choice><reg>διακονητῶν</reg><orig>διακονιτῶν</orig></choice>

    <lb n="16"/>τῆς <choice><reg>Ἀνταιοπολιτῶν</reg><orig>Ἀνταιουπολιτῶν</orig></choice> <expan><ex>ὑπὲρ</ex></expan> ἐμβολῆ<unclear>ς</unclear> <unclear>τ</unclear>ῆς αὐτῆς κώμης <choice><reg>κανόνος</reg><orig>κανόνα<unclear>ς</unclear></orig></choice> <num value="7">ἑβδόμης</num> ἰνδικτί<supplied reason="lost">ο</supplied><unclear>νος</unclear>

    <lb n="17"/>σίτου καθαροῦ ἀρτάβας <num value="5759"><choice><reg>πεντακισχιλίας</reg><orig>πεντακεσχιλίας</orig></choice> ἑπτακοσίας πεντήκοντα ἐννέα</num>

    <lb n="18"/><expan>σ<unclear>ὺ</unclear><ex>ν</ex></expan> <expan><unclear>να</unclear><ex>ύ</ex><supplied reason="lost">λ<ex>οις</ex></supplied></expan> <expan><unclear>κ</unclear><ex>αὶ</ex></expan> <num value="1/100"><choice><reg>ἑκατοσταῖς</reg><orig>ἑκατεστῆς</orig></choice></num> <expan>κ<ex>αὶ</ex></expan> <num value="1/20"><choice><reg>εἰκοσταῖς</reg><orig>εἰκοστ<unclear>ῆς</unclear></orig></choice></num>, <expan>γί<ex>νονται</ex></expan> <expan>σίτ<ex>ου</ex></expan> <expan>καν<ex>όνος</ex></expan> <expan><ex>ἀρτάβαι</ex></expan> <num value="5759">Εψνθ</num> <expan>μόν<ex>αι</ex></expan>. καὶ εἰς ὑμῶν ἀσφάλειαν

    <lb n="19"/>καὶ τοῦ <choice><reg>δημοσί<supplied reason="lost">ου</supplied></reg><orig>τεμ<unclear>οσί</unclear><supplied reason="lost">ου</supplied></orig></choice> <choice><reg>πεποίημαι</reg><orig><unclear>πεποίμε</unclear></orig></choice> <unclear>τ</unclear>ὴν <choice><reg>ἀπόδειξιν</reg><orig>ἀπόδιξι<unclear>ν</unclear></orig></choice> <unclear>ὡς</unclear> <expan><unclear>πρόκ</unclear><ex>ειται</ex></expan>. <supplied reason="lost">Ἀναστάσιος καὶ Ἀναστάσιος <expan>διακ<ex>ονηταὶ</ex></expan></supplied>

    <lb n="20"/><expan>δ<ex>ιὰ</ex></expan> ἐμοῦ Τιμοθέου <choice><reg>γραμματέως</reg><orig><expan>γραματ<ex>έως</ex></expan></orig></choice> τῆς αὐτῆς κώμης <choice><reg>πεποιήμεθα</reg><orig>πεποίμε</orig></choice> τὴν <choice><reg>ἀπόδειξιν</reg><orig>ἀπότηξιν</orig></choice> τῶν σί<unclear>τ</unclear><supplied reason="lost">ο</supplied>υ <unclear>ἀρταβῶν</unclear>

    <lb n="21"/><num value="5000"><choice><reg>πεντακισχιλίων</reg><orig>πεντακεσχιλίας</orig></choice></num> <num value="700"><choice><reg>ἑπτακοσίων</reg><orig>ἑπτακοσίας</orig></choice></num> <num value="50">πεντήκοντα</num> <num value="9">ἐννέα</num> <add place="above">καὶ το<unclear>ῦ</unclear> <unclear>ν</unclear>α<supplied reason="lost">ύ</supplied><unclear>λ</unclear><supplied reason="lost">ου</supplied></add> ὡς <expan>πρόκ<ex>ειται</ex></expan>. <expan>ἐγράφ<ex>η</ex></expan> <expan>Μεσορ<ex>η</ex></expan> <num value="6">ϛ</num>

    <lb n="22"/><expan>ἰνδικ<ex>τίονος</ex></expan> <num value="7">ζ</num>.
