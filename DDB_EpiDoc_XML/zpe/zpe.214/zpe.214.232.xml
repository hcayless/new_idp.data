         <head n="20683" xml:lang="en">
            <date>AD 126</date>
            <placeName>Oxyrhynchus</placeName>
            <ref n="p.oxy;3;634" type="reprint-from">P.Oxy. 3 634</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve">
<ab>
    <lb n="1"/>ἔτους <num value="11">ἑνδεκάτου</num> Αὐτοκράτορος Καίσαρος Τραιανοῦ Ἁδριανοῦ Σεβαστοῦ, μηνὸς Νέου Σεβαστοῦ <num value="11"><hi rend="supraline">ια</hi></num>, ἐν Ὀξυρύγχ<supplied reason="lost">ων</supplied> <supplied reason="lost">πόλει τῆς Θηβαΐδος, ἀγαθῇ τύχῃ.</supplied> <supplied reason="lost"><space extent="unknown" unit="character"/></supplied>

    <lb n="2"/>τάδε διέθετο νοοῦσα καὶ φρονοῦσα Ἅυγχις Ἰ<unclear>σ</unclear>ᾶ<unclear>το</unclear>ς τοῦ Θοτέως μητρὸς Τρυφᾶτος τῆς καὶ Κου<gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">μετὰ κυρίου τοῦ ἀνεψιοῦ</supplied> <gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">τοῦ Θοτέως μητρὸς</supplied> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="3"/>ἀμφότεροι τῶν ἀπ’ Ὀξυρύγχων πόλεως ἐν ἀγυιᾷ. ἐφ’ ὃν μὲν περίειμι χρόνον ἔχειν με τὴν κατὰ τ<supplied reason="lost">ῶν ἰδίων ἐξουσίαν ὃ ἐὰν βούλωμαι ἐπιτελεῖν καὶ μεταδιατίθεσθαι καὶ ἀκυροῦν τὴν διαθήκην ταύτην, ὃ δʼ ἂν ἐπιτελέσω κύριον ὑπάρχειν. ἐὰν δὲ</supplied>

    <lb n="4"/>ἐπὶ ταύτῃ τελευτήσω τῇ διαθήκῃ, κληρον<unclear>ό</unclear>μο<unclear>ν</unclear> ἀπολείπω τὸν ἄνδρα μου Ἀβάσκαντον ἀπελεύθ<supplied reason="lost">ερον Ἀσκληπιάδου</supplied> <gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">τῆς ὑπαρχούσης μοι ἐν</supplied> <expan><supplied reason="lost">Ὀξυ</supplied>

    <lb n="5" break="no"/><unclear>ρύγ</unclear>χ<ex>ων</ex></expan> πόλει ἐπ’ ἀμφόδ<unclear>ο</unclear>υ Τεμγενούθεως οἰκίας <supplied reason="lost">κ</supplied><unclear>α</unclear>ὶ τ<supplied reason="lost">ῶν</supplied> <supplied reason="lost">τ</supplied>αύτης χρηστηρίων καὶ  ὧν ἄλλων ἐὰν ἀπολί<unclear>π</unclear><supplied reason="lost">ω</supplied> <gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">ἐπὶ τῷ τὸν αὐτὸν Ἀβάσκαντον δοῦναι ἀργυρίου δραχμὰς</supplied> <num><gap reason="lost" extent="unknown" unit="character"/></num> <supplied reason="lost">Τυχα</supplied>

    <lb n="6" break="no"/>ρο<unclear>ῦ</unclear>τι δ<unclear>ο</unclear>ύλῃ Διογένους <unclear>Ἡ</unclear>ρακλείδου τοῦ Μιγκίωνος ἀπὸ τῆς αὐτῆς πόλεως, ἐὰν ζῇ, ἥτις καὶ ἕξει ἐπὶ τὸν τ<supplied reason="lost">ῆς ζωῆς αὐτῆς χρόνον τὴν χρῆσιν καὶ ἐνοίκησιν</supplied> <gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">τῆς <num value="2">δευτέρας</num></supplied>

    <lb n="7"/><supplied reason="lost">σ</supplied><unclear>τέ</unclear>γης ὄντος ἐ<supplied reason="lost">π</supplied><unclear>ὶ</unclear> <unclear>τ</unclear><supplied reason="lost">οῦ</supplied> πεσσοῦ τῆς προκειμένης οἰκίας, οὐκ ἔχουσα ἐξουσίαν οὐδένα ἄλλον <choice><reg>ἐπεισά<supplied reason="lost">γειν</supplied></reg><orig>ἐπισά<supplied reason="lost">γειν</supplied></orig></choice> <gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">ἐὰν δὲ συμβῇ ἐν ἐνιαυτῷ <num value="1">ἑνὶ</num> ἀφ’ οὗ ἐὰν τελευτήσω τὴν Τυχα</supplied>

    <lb n="8" break="no"/>ροῦν ἐλευθέραν γενέ<unclear>σθ</unclear>αι, δώσει αὐτῇ ὁ Ἀβάσκαν<unclear>τος</unclear> ἐν τῷ αὐτῷ ἐνιαυτῷ ἄλλας ἃς διατάσσ<unclear>ω</unclear> <supplied reason="lost">αὐτῇ ἀργυρίου δραχμὰς</supplied> <num value="500"><supplied reason="lost">πεντακοσίας</supplied></num> <gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">ἐὰν δὲ συμβῇ περιούσης μου ἢ μετὰ τὸν</supplied>

    <lb n="9"/>ὡρ<unclear>ι</unclear>σθέντα ἐνιαυτὸν τὴν αὐτὴν Τυχαροῦν ἐλευ<unclear>θ</unclear><supplied reason="lost">έ</supplied>ραν <supplied reason="lost">γε</supplied><unclear>ν</unclear>έσθαι ἢ μὴ <unclear>ἄλ</unclear>λως ἐλευθερωθῆναι <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">καὶ μὴ ἐξεῖναι</supplied>

    <lb n="10"/><supplied reason="lost">μ</supplied><unclear>ηδ</unclear>ενὶ ἄλλῳ <choice><reg>παρεγχειρεῖν</reg><orig>παρενχιρεῖν</orig></choice> τοῖς ὑπ’ ἐμοῦ δ<supplied reason="lost">ι</supplied>ατεταγμένοις ἢ τὸν παραβάντα ἀποτίνειν τῷ κληρ<unclear>ον</unclear><supplied reason="lost">όμῳ</supplied> <gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">κυρία</supplied>

    <lb n="11"/>ἡ διαθήκη. <handShift new="m2"/>Ἅυγχι<unclear>ς</unclear> <unclear>Ἰσᾶ</unclear>τος τοῦ Θ<unclear>οτ</unclear>έως π<unclear>ε</unclear>ποίημαι τὴν διαθήκην κα<unclear>ὶ</unclear> <supplied reason="lost">καταλείπω  μετὰ τὴν τελευτὴν κληρονόμον τὸν ἄνδρα μου Ἀβάσκα</supplied>

    <lb n="12" break="no"/><supplied reason="lost">ν</supplied><unclear>τ</unclear>ον ἀπελ<unclear>ε</unclear><supplied reason="lost">ύ</supplied><unclear>θερο</unclear>ν Ἀ<unclear>σκ</unclear><supplied reason="lost">λ</supplied><unclear>η</unclear>πιάδου ἧς ἔχω ἐπὶ Τεμγε<unclear>ν</unclear><supplied reason="lost">ούθεως ἀμφόδου οἰκίας</supplied> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="13"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>λιου</unclear> α<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><unclear>τοι</unclear><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>υν</unclear><gap reason="illegible" quantity="1" unit="character"/>ι<gap reason="illegible" quantity="1" unit="character"/>ει<gap reason="illegible" quantity="1" unit="character"/><unclear>ν</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><unclear>ε</unclear>στω τυ<unclear>χ</unclear><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="14"/><supplied reason="lost">ἐν</supplied><unclear>ο</unclear>ίκη<unclear>σιν</unclear> οἴκου ε<gap reason="illegible" quantity="5" unit="character"/>ρου τῆς <num value="2">δευτέρα</num> <unclear>σ</unclear>τέ<unclear>γη</unclear><supplied reason="lost">ς</supplied> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="15"/><gap reason="lost" quantity="2" unit="character"/>τειν, <choice><reg>ἐὰν</reg><orig>αἰὰν</orig></choice> δὲ ἐν ἐνιαυτῷ ἑνὶ ἀφ’ οὗ <choice><reg>ἐὰν</reg><orig>αἰὰν</orig></choice> τελευτ<supplied reason="lost">ήσω</supplied> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="16"/><supplied reason="lost">δρ</supplied>αχμὰς  <num value="500">πεντακοσίας</num>, <choice><reg>ἐὰν</reg><orig>αἰὰν</orig></choice> δὲ περιούσης μου ἢ μετὰ ἐ<supplied reason="lost">νιαυτὸν</supplied> <num value="1"><supplied reason="lost">ἕνα</supplied></num> <supplied reason="lost">τῆς τελευτῆς μου</supplied> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="17"/><gap reason="lost" quantity="2" unit="character"/> <gap reason="illegible" quantity="12" unit="character"><desc>vestiges</desc></gap> <unclear>η</unclear><gap reason="illegible" quantity="1" unit="character"/><unclear>υ</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>ι</unclear><gap reason="illegible" quantity="1" unit="character"/> <unclear>α</unclear>ὐτῇ οὐδ’ ἄλλῳ ὑπὲ<unclear>ρ</unclear> <supplied reason="lost">αὐτῆς</supplied> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="18"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ν ὡς πρό<supplied reason="lost">κε</supplied><unclear>ι</unclear>ται. εἰμὶ ἐτῶν <num value="46">μϛ</num>, χωλαίνουσα τὸ<unclear>ν</unclear> <gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">καὶ ἔστι μου ἡ σφραγὶς</supplied> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="19"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><unclear>ψε</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/><unclear>τ</unclear>ος τοῦ Θ<unclear>οτ</unclear><supplied reason="lost">έως ὁ</supplied> <unclear>ἐ</unclear><supplied reason="lost">κ τ</supplied><unclear>ο</unclear>ῦ πατρὸς αὐτῆς ἀνεψ<supplied reason="lost">ιὸς ἐπιγέγραμμαι αὐτῆς κύριος καὶ ἔγραψα ὑπὲρ αὐτῆς</supplied>

    <lb n="20"/><supplied reason="lost">μὴ</supplied> <supplied reason="lost">εἰδυία</supplied><unclear>ς</unclear> γρ<unclear>ά</unclear>μ<unclear>μα</unclear><supplied reason="lost">τ</supplied><unclear>α</unclear> καὶ <unclear>ε</unclear>ἰμὶ <expan><ex>ἐτῶν</ex></expan> <num value="57">νζ</num>, οὐλὴ δακτύ<unclear>λ</unclear><supplied reason="lost">ῳ</supplied> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="21"/><handShift new="m3"/><gap reason="lost" extent="unknown" unit="character"/>  <supplied reason="lost">ἀπ</supplied>ὸ τῆς αὐτῆς <unclear>π</unclear><supplied reason="lost">όλ</supplied><unclear>ε</unclear>ως μαρτυρῶ τῇ τῆ<unclear>ς</unclear> Ἁύγχιος διαθήκῃ καὶ εἰμὶ ἐτῶν <num value="65">ἑξήκοντα πέ<supplied reason="lost">ντε</supplied></num> <gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">καὶ ἔστι μου ἡ σφραγὶς</supplied> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="22"/><handShift new="m4"/><gap reason="lost" extent="unknown" unit="character"/><unclear>σ</unclear>του ἀπὸ τῆς αὐτῆς <unclear>π</unclear><supplied reason="lost">ό</supplied>λεως μαρτυρῶ τῇ τῆς Ἁύγχιος διαθήκῃ καὶ εἰμὶ ἐτῶν <gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">καὶ ἔστι μου ἡ σφραγὶς</supplied> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="23"/><handShift new="m5"/><gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">Ἀ</supplied>μμωνίου ἀπὸ τῆς <unclear>αὐτ</unclear>ῆς πόλεως μαρτυρῶ τῇ τῆς Ἁύγχιος διαθήκῃ καὶ ε<supplied reason="lost">ἰμὶ</supplied> <supplied reason="lost">ἐτῶν</supplied> <gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">καὶ ἔστι μου ἡ σφραγὶς</supplied> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="24"/><handShift new="m6"/><gap reason="lost" extent="unknown" unit="character"/> <gap reason="illegible" extent="unknown" unit="character"><desc>vestiges</desc></gap> <supplied reason="lost">μαρτ</supplied><unclear>υρ</unclear>ῶ <unclear>τ</unclear>ῇ τῆ<unclear>ς</unclear> <supplied reason="lost">Ἁύγχ</supplied>ιο<unclear>ς</unclear> <supplied reason="lost">δι</supplied><unclear>α</unclear>θήκῃ καὶ εἰμὶ ἐ<unclear>τ</unclear>ῶν <num value="30">τριάκοντα</num> <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">καὶ ἔστι μου ἡ σφραγὶς</supplied> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="25"/><handShift new="m7"/><gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">ἀπὸ</supplied> <unclear>τῆς</unclear> <unclear>αὐτ</unclear>ῆ<supplied reason="lost">ς π</supplied><unclear>ό</unclear>λε<unclear>ω</unclear><supplied reason="lost">ς μ</supplied><unclear>α</unclear>ρ<unclear>τ</unclear>υρ<unclear>ῶ</unclear> τῇ τῆς Ἁύγχιος δια<supplied reason="lost">θήκῃ καὶ εἰμὶ ἐτῶν</supplied> <gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">καὶ ἔστι μου ἡ σφραγὶς</supplied> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="26"/><handShift new="m8"/><gap reason="lost" extent="unknown" unit="character"/>ιων <unclear>Σ</unclear><supplied reason="lost">α</supplied>ρα<supplied reason="lost">πί</supplied><unclear>ω</unclear>νος ἀπὸ τῆς αὐτῆς πόλεως μαρτ<unclear>υ</unclear><supplied reason="lost">ρῶ</supplied> <supplied reason="lost">τῇ τῆς Ἁύγχιος διαθήκῃ καὶ εἰμὶ ἐτῶν</supplied> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="27"/><gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">καὶ</supplied> ἔστι <unclear>μ</unclear><supplied reason="lost">ου ἡ σφραγὶς</supplied> προτομὴ Ἐνκανώπου.

    <lb n="27"/><space extent="unknown" unit="character"/>

    <lb n="28"/><space extent="unknown" unit="character"/> <handShift new="m9"/><expan>μνημ<ex>ονείου</ex></expan> <expan>Ὀξ<ex>υρύγχων</ex></expan> <expan>π<unclear>ό</unclear>λ<ex>εως</ex></expan>.

    <lb n="29"/><supplied reason="lost">ἔτους <num value="11">ἑνδεκάτου</num> Αὐτοκράτορος Καίσαρος</supplied> Τ<unclear>ρ</unclear><supplied reason="lost">αι</supplied><unclear>α</unclear>νοῦ Ἁδ<unclear>ρ</unclear>ια<unclear>νο</unclear>ῦ Σεβαστ<unclear>ο</unclear><supplied reason="lost">ῦ</supplied>, <supplied reason="lost">μ</supplied><unclear>ηνὸς</unclear> <unclear>Νέ</unclear><supplied reason="lost">ου</supplied> <supplied reason="lost">Σεβαστοῦ <num value="11"><hi rend="supraline">ια</hi></num>.</supplied> <supplied reason="lost"><space extent="unknown" unit="character"/></supplied>

    <lb n="30"/><supplied reason="lost">διαθήκη Ἁύγχιος Ἰσᾶτος τ</supplied><unclear>ο</unclear>ῦ Θοτέως μητρὸς <unclear>Τ</unclear>ρυ<unclear>φᾶ</unclear><supplied reason="lost">τος τῆς καὶ Κου</supplied><gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">ἀπ’ Ὀξυρύγχων πόλεως.</supplied> <supplied reason="lost"><space extent="unknown" unit="character"/></supplied>

