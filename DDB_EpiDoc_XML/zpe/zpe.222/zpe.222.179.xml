         <head n="20593" xml:lang="en">
            <date>AD 79-80</date>
            <placeName>Oxyrhynchus</placeName>
            <ref n="p.oxy;2;373" type="reprint-from">P.Oxy. 2 373</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve">
<ab>
    <lb n="1"/><supplied reason="lost">ἔτους</supplied> <num value="2"><supplied reason="lost">δε</supplied>υτέρου</num> Αὐτοκράτορος Τίτου Καίσαρ<supplied reason="lost">ος Οὐεσπασιανοῦ Σεβαστοῦ</supplied> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2" rend="indent"/><num value="30"><unclear>λ</unclear></num> <unclear>ἐν</unclear> Ὀξυρύγχων πόλει τῆς Θη<unclear>βα</unclear><supplied reason="lost">ΐδος.</supplied>

    <lb n="3"/><choice><reg>ἐδάνεισεν</reg><orig>ἐδάνισεν</orig></choice> <supplied reason="lost">Σε</supplied><unclear>λ</unclear>ήνη Ἥρωνος τοῦ Ἥρωνος μητ<supplied reason="lost">ρὸς</supplied> <gap reason="lost" quantity="13" unit="character" precision="low"/> <supplied reason="lost">τῶν ἀπ’ Ὀ</supplied>

    <lb n="4" break="no"/>ξυρύγχων πόλεως μετὰ κυρίου τοῦ ἑαυτῆ<supplied reason="lost">ς</supplied> <supplied reason="lost"><gap reason="illegible" quantity="22" unit="character" precision="low"/> </supplied>

    <lb n="5"/>τοῦ Ψά<unclear>μμ</unclear>ιδος μητρὸς Διδύμης τῆ<unclear>ς</unclear> <gap reason="lost" quantity="5" unit="character" precision="low"/> <supplied reason="lost">τῶν ἀπὸ τῆς αὐτῆς πόλεως</supplied>

    <lb n="6"/>Ἀπολλω<supplied reason="lost">νί</supplied>ᾳ Δημητρίου ἀστῇ <choice><reg>Περσίνῃ</reg><orig>Περσείνῃ</orig></choice> <supplied reason="lost">τῆ</supplied>ς ἐ<unclear>π</unclear><supplied reason="lost">ιγονῆς μετὰ κυρίου</supplied> <supplied reason="lost">6-7</supplied>

    <lb n="7" break="no"/>κλέους <supplied reason="lost">τοῦ</supplied> Θεμιστοκλέους Καισαρείου τοῦ καὶ <unclear>Ζ</unclear><supplied reason="lost">ηνείου ἐν ἀγυιᾷ ἀργυρίου</supplied>

    <lb n="8"/>Σεβαστοῦ νομίσματος δραχμὰς <num value="1120">χιλίας ἑκατ<unclear>ὸ</unclear><supplied reason="lost">ν εἴκοσι</supplied></num> <supplied reason="lost">κεφαλαίου, αἷς οὐδὲν τῷ</supplied>

    <lb n="9"/>καθόλου προσῆκται· ἀποδότω δὲ ἡ <choice><reg>δεδανεισμ<supplied reason="lost">ένη</supplied></reg><orig>δεδανισ<unclear>μ</unclear><supplied reason="lost">ένη</supplied></orig></choice> <supplied reason="lost">τῇ Σελήνῃ τὰς τοῦ ἀργυρί</supplied>

    <lb n="10" break="no"/>ου δραχμὰς <num value="1120">χιλίας ἑκατὸν εἴκοσι</num> τῇ <num value="30">τρ<unclear>ια</unclear>κ<supplied reason="lost">άδι</supplied></num> <supplied reason="lost">τοῦ</supplied> <supplied reason="lost">5-7</supplied> <supplied reason="lost">μηνὸς τοῦ</supplied> <supplied reason="lost">6-8</supplied>

    <lb n="11"/>ἔτους Αὐτοκράτορος Τίτου Καίσαρος Οὐεσπασ<unclear>ι</unclear><supplied reason="lost">ανοῦ χωρὶς πάσης ὑ</supplied>

    <lb n="12" break="no"/>περθέσεως. ἐὰν δὲ μὴ ἀποδῷ καθὰ γέγραπ<supplied reason="lost">ται, συγχωρεῖ ἡ δεδανεισμέ</supplied>

    <lb n="13" break="no"/>νη μένειν περὶ τὴν Σελήνην καὶ τοὺς παρ’ αὐ<supplied reason="lost">τῆς μεταληψομένους ἀντὶ</supplied>

    <lb n="14"/>τοῦ προκειμένου κεφαλαίου ἀπὸ τοῦ ὡρισμέ<supplied reason="lost">νου τῆς ἀποδόσεως χρόνου τὴν</supplied>

    <lb n="15"/>κράτησιν καὶ <choice><reg>κυρίαν</reg><orig>κυρείαν</orig></choice> εἰς τὸν ἀεὶ <unclear>χ</unclear>ρόνον κα<supplied reason="lost">τοικικῆς καὶ ὠνημένης εἰς κατοικί</supplied>

    <lb n="16" break="no"/>αν γῆς σιτοφόρου σπορίμο<supplied reason="lost">υ</supplied> ἐξ <choice><reg>ὀ<supplied reason="lost">ρ</supplied>θογωνίου</reg><orig>ὠ<supplied reason="lost">ρ</supplied>θογωνείου</orig></choice> ἀρ<supplied reason="lost">ουρῶν <num value="10">δέκα</num> ἀπὸ τῶν ὑπαρχουσῶν</supplied>

    <lb n="17"/>αὐτῇ περὶ <choice><reg>Σιναρυ</reg><orig>Σιναροι</orig></choice> τῆς κ<supplied reason="lost">άτω τοπ</supplied>αρχίας ἐκ το<supplied reason="lost">ῦ</supplied> <supplied reason="lost">7-10</supplied> ἀρουρῶν <supplied reason="lost">4-7</supplied>

    <lb n="18"/>κοντα ἑπτὰ ἀφ’ ἧς ἐὰν αἱ<supplied reason="lost">ρῆται α</supplied>ὐτῶν ἡ Σελή<supplied reason="lost">νη</supplied> <gap reason="lost" quantity="25" unit="character" precision="low"/>

    <lb n="19"/>ται σχοινισμούς· γείτο<supplied reason="lost">νε</supplied><unclear>ς</unclear> <unclear>τῶν</unclear> ἀρου<supplied reason="lost">ρῶν νότου</supplied> <gap reason="lost" quantity="20" unit="character" precision="low"/>

    <lb n="20"/>βορρᾶ γύης, ἀπηλιώτου πλευρισμός, λιβὸς θε<gap reason="lost" quantity="20" unit="character" precision="low"/>

    <lb n="21"/>καὶ <choice><reg>ἐξεῖναι</reg><orig>ἐξῖναι</orig></choice> τ<supplied reason="lost">ῇ</supplied> <unclear>Σ</unclear>ελήνῃ ἀπὸ τῆσδε τῆς <choice><reg>συγ<supplied reason="lost">γραφῆς</supplied></reg><orig><unclear>σ</unclear>υ<unclear>ν</unclear><supplied reason="lost">γραφῆς</supplied></orig></choice> <supplied reason="lost">μετεπιγράφεθαι</supplied>

    <lb n="22"/>διὰ τῶν καταλοχισμῶν τὰς <num value="10">δέκα</num> ἀρούρας, μ<supplied reason="lost">ὴ προσδεηθείσῃ τῆς τῆς δεδανεισ</supplied>

    <lb n="23" break="no"/>μένης παρουσίας μηδὲ συνεπιγραφῆς κ<unclear>α</unclear><supplied reason="lost">ὶ ἀποφέρεσθαι τὰ ἐξ αὐτῶν περι</supplied>

    <lb n="24" break="no"/>εσόμενα <supplied reason="lost">κα</supplied><unclear>ὶ</unclear> χρᾶσθαι καὶ οἰκονομεῖν πε<unclear>ρ</unclear><supplied reason="lost">ὶ αὐτῶν καθ’ ὃν ἐὰν αἱρῆται τρό</supplied>

    <lb n="25" break="no"/>πον, ἐφ’ ἃς καὶ μηδεμίαν τῇ δεδανισμέ<supplied reason="lost">νῃ ἢ τοῖς παρ’ αὐτῆς μεταληψομένοις</supplied>

    <lb n="26"/>καταλεί<unclear>πε</unclear>σθαι ἔφοδον ἀλλὰ καὶ ἐπάναγκ<supplied reason="lost">ον τὴν δεδανεισμένην παρέξεσθαι</supplied>

    <lb n="27"/>τῇ Σελήνῃ καὶ τοῖς παρ’ αὐτῆς διὰ πα<supplied reason="lost">ντὸς μὲν βεβαίας, καθαρὰς δὲ καὶ</supplied>

    <lb n="28"/>ἀπὸ δημοσί<unclear>ων</unclear> <supplied reason="lost">καὶ τῶν ἄλλων τελεσμάτων πάντων τῶν ἔμπροσθεν</supplied>

    <lb n="29"/><unclear>χ</unclear>ρόνων μ<unclear>έ</unclear><supplied reason="lost">χρι</supplied> <gap reason="lost" quantity="50" unit="character" precision="low"/>

    <lb n="30"/>ἀπὸ δὲ τοῦ <unclear>ν</unclear><supplied reason="lost">ῦν</supplied> <gap reason="lost" quantity="50" unit="character" precision="low"/>

    <lb n="31"/>αὐτῶν οὔτ<unclear>ε</unclear> <gap reason="lost" quantity="50" unit="character" precision="low"/>

    <lb n="32"/>κατ’ οὐδένα <supplied reason="lost">τρόπον</supplied> <gap reason="lost" quantity="45" unit="character" precision="low"/>

    <lb n="33"/>ὡς δεδήλωτ<supplied reason="lost">αι</supplied> <gap reason="lost" quantity="50" unit="character" precision="low"/>

    <lb n="34"/><gap reason="illegible" quantity="2" unit="character"/><unclear>α</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="55" unit="character" precision="low"/>

    <lb n="34"/><gap reason="lost" extent="unknown" unit="line"/>
</ab></div>
