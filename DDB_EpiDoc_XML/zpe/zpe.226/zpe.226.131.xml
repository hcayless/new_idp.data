<ab>
    <lb n="1"/><g type="stauros"/> <expan>ἐδόθ<ex>η</ex></expan> <expan>δ<ex>ιὰ</ex></expan> Ἀπολλῶ <expan>προ<ex>νοητοῦ</ex></expan> Νίκης <space extent="unknown" unit="character"/> Στεφάνῳ <expan>ποταμ<ex>ίτῃ</ex></expan> καὶ τοῖς <choice><reg>ἑταίροις</reg><orig><expan>ἑτέρ<ex>οις</ex></expan></orig></choice> <expan>αὐτ<ex>οῦ</ex></expan> <expan>ἐργ<unclear>α</unclear><supplied reason="lost">ζομέ<ex>νοις</ex></supplied></expan> <gap reason="lost" extent="unknown" unit="character"/>  

    <lb n="2"/><abbr>λάκκ</abbr> ἐν <expan>κτήμ<ex>ατι</ex></expan> Νίκης ἐν τῇ <expan>μηχ<ex>ανῇ</ex></expan> <expan>καλουμ<ex>ένῃ</ex></expan> <subst><add place="inline">Πατνηα</add><del rend="corrected">π<gap reason="illegible" quantity="5" unit="character"/></del></subst> ἐπὶ τῆς <num value="6"><hi rend="supraline">ϛ</hi></num> <expan>ἰνδ<ex>ικτίωνος</ex></expan> <expan>γεωργουμ<ex>ένῃ</ex></expan> <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>τοῦ <expan>αὐ<ex>τοῦ</ex></expan> <expan>κτήμ<ex>ατος</ex></expan> καὶ Μαξίμου <expan>μείζ<ex>ονος</ex></expan> <expan>λόγ<ex>ῳ</ex></expan> <expan>μισθ<ex>οῦ</ex></expan> ὑπὲρ <choice><reg>ναυβίων</reg><orig><expan>ναυεί<ex>ων</ex></expan></orig></choice> <num value="166">ρξϛ</num> ἐξ <expan><ex>ὧν</ex></expan> <expan>κουφίζ<ex>ονται</ex></expan> <expan>ὑ<ex>πὲρ</ex></expan> τοῦ <expan>παλαι<ex>οῦ</ex></expan> <expan>λάκκ<ex>ου</ex></expan> <choice><reg>ναύβια</reg><orig><expan>ναύει<ex>α</ex></expan></orig></choice> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>εἰς <expan>νο<ex>μισμάτια</ex></expan> <num value="4">δ</num> <num value="1/6" rend="tick">ϛ</num> <expan>π<ex>αρὰ</ex></expan> <num value="19">ιθ</num> καὶ σίτ<unclear>ου</unclear> <expan><ex>ἀρτάβας</ex></expan> <num value="4">δ</num> καὶ <expan>ἀρ<ex>γυρίου</ex></expan> <expan><ex>δηναρίων μυριάδας</ex></expan> <num value="1200">Ασ</num>, ἐξ ὧν <expan>ἐδόθ<ex>η</ex></expan> <expan>αὐτ<ex>οῖς</ex></expan> πρώην <expan>νο<ex>μισμάτια</ex></expan> <num value="2">β</num> <expan>π<ex>αρὰ</ex></expan> <num value="9">θ</num>, <expan>λοιπ<ex>ὰ</ex></expan> ν<unclear>ομ</unclear><supplied reason="lost">ισμάτια <num value="2">δύο</num> <num value="1/4">τέταρτον</num></supplied>

    <lb n="5"/><expan>παρ<ex>ὰ</ex></expan> δέκα καὶ σίτου <choice><reg>ἀρτάβαι</reg><orig>ἀρτάβας</orig></choice> <num value="4"><choice><reg>τέσσαρες</reg><orig>τέσσαρας</orig></choice></num> καὶ <expan>ἀρ<ex>γυρίου</ex></expan> <choice><reg><expan><ex>μυριάδες</ex></expan></reg><orig><expan><ex>μυριάδας</ex></expan></orig></choice> <num value="1200"><choice><reg>χίλιαι</reg><orig>χιλίας</orig></choice> <choice><reg>διακόσιαι</reg><orig>διακοσίας</orig></choice></num>, <expan>γί<ex>νονται</ex></expan> <expan>χρ<ex>υσοῦ</ex></expan> <expan>ἰδ<ex>ιωτικῷ</ex></expan> <expan>νο<ex>μισμάτια</ex></expan> <num value="2">β</num> <num value="1/4" rend="tick">δ</num> <expan>π<ex>αρὰ</ex></expan> <expan>κερ<ex>άτια</ex></expan> <num value="10">ι</num> καὶ <expan>σίτ<ex>ου</ex></expan> <expan><ex>ἀρτάβαι</ex></expan> <num value="4">δ</num> <unclear>κ</unclear><supplied reason="lost">αὶ</supplied> <supplied reason="lost"><expan>ἀρ<ex>γυρίου</ex></expan> <expan><ex>μυριάδες</ex></expan> <num value="1200">Ασ</num> <expan>μ<ex>όνα</ex></expan>.</supplied> 

    <lb n="6"/><expan><ex>ἔτους</ex></expan> <num value="249"><hi rend="supraline">σμθ</hi></num> <num value="218"><hi rend="supraline">σιη</hi></num> Παῦνι <num value="19">ι<hi rend="supraline">θ</hi></num> <expan>ἰνδ<ex>ικτίωνος</ex></expan> <num value="6">ἕκτης</num>. <g type="stauros"/> <handShift new="m2"/><expan>γί<ex>νονται</ex></expan> <expan>χρ<ex>υσοῦ</ex></expan> <expan>ἰδ<ex>ιωτικῷ</ex></expan> <expan>ζυγ<ex>ῷ</ex></expan> νομισμάτια <num value="2">δύο</num> <num value="1/4">τέταρτον</num> παρὰ <expan>κερ<ex>άτια</ex></expan> <num value="10">δέκ<unclear>α</unclear></num> <supplied reason="lost">καὶ <expan>σίτ<ex>ου</ex></expan></supplied> 

    <lb n="7"/><space extent="unknown" unit="character"/> <choice><reg>ἀρτάβαι</reg><orig>ἀρτάβας</orig></choice> <num value="4"><choice><reg>τέσσαρες</reg><orig>τέσσαρας</orig></choice></num> καὶ <expan>ἀργυρ<ex>ίου</ex></expan> <choice><reg><expan>μυριάδ<ex>ες</ex></expan></reg><orig><expan><ex>μυριάδας</ex></expan></orig></choice> <num value="1200"><choice><reg>χίλιαι</reg><orig>χιλίας</orig></choice> <choice><reg>διακόσια<supplied reason="lost">ι</supplied></reg><orig>διακοσία<supplied reason="lost">ς</supplied></orig></choice></num> <supplied reason="lost"><expan>μ<ex>όνα</ex></expan>.</supplied>  
