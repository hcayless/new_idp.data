    <lb n="3"/><supplied reason="lost">πατ</supplied>ρὸς Πολυδεύκ<supplied reason="lost">ους, οἱ τέσσαρε</supplied>ς <num value="4"/> Ἑρμοπολεῖ<supplied reason="lost">τ</supplied>αι, <app type="editorial"><lem>Ἑρμ<supplied reason="lost">ῇ</supplied></lem><rdg>Ερμ<gap reason="lost" quantity="2" unit="character"/></rdg></app><supplied reason="lost"> καὶ </supplied><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>ίωνι</unclear> <unclear>κ</unclear>α<unclear>ὶ</unclear> <gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> <supplied reason="lost">τοῖ</supplied><unclear>ς</unclear> τρισὶ <num value="3"/> μητρα

