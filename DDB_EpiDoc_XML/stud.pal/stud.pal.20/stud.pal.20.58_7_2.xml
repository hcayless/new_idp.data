         <head n="31024" xml:lang="en">
            <date>c. AD 265-6</date>
            <placeName>Hermoupolis Magna</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="FrA,2" type="textpart"><ab>

    <lb n="1"/><num value="64">ξδ</num> 

    <lb n="1"/>τῇ κρατίστῃ βουλῇ Ἑρμουπόλεως <supplied reason="lost">τ</supplied>ῆς μεγάλης ἀρχαίας καὶ λαμπρᾶς <app type="editorial"><lem resp="BL cf. 1.412"><supplied reason="lost">διὰ</supplied></lem><rdg><supplied reason="lost">παρὰ</supplied></rdg></app><supplied reason="lost"> Μ</supplied>άρκου Αὐρη

    <lb n="2" break="no"/>λίου Ἀσκληπιάδου τοῦ καὶ Ἑρμ<supplied reason="lost">ο</supplied>δώρου νεωκόρου τοῦ μεγάλου Σαρ<supplied reason="lost">άπιδ</supplied>ος πανκρα

    <lb n="3" break="no"/>τιαστοῦ <choice><reg>περιοδονίκου</reg><orig>περιοδονείκου</orig></choice> ἀλείπτου ἀ<supplied reason="lost">συνε</supplied>ξώστου διὰ βίου ἐνάρχου π<unclear>ρυτά</unclear><supplied reason="lost">νεως <app type="editorial"><lem resp="BL cf. 1.412">παρὰ</lem><rdg>διὰ</rdg></app></supplied> 

    <lb n="4"/>Μάρκων Αὐρηλίων Ἀμμωνίο<supplied reason="lost">υ τοῦ</supplied> καὶ <unclear>Π</unclear>ολυδεύ<supplied reason="lost">κ</supplied>ους <expan>κοσ<ex>μητεύσαντος</ex></expan> καὶ Διοσκόρου Διοσκόρου 

    <lb n="5"/>Ἑρμαίου ἀγορανομήσαντος <expan>ἀμφοτ<ex>έρων</ex></expan> <expan>βουλ<ex>ευτῶν</ex></expan> τῆς αὐτῆς πόλεως· ἐπεὶ ᾑρέθημεν ἐν τῷ συν

    <lb n="6" break="no"/>εδρίῳ <expan>ἀκολ<ex>ούθως</ex></expan> τοῖς <expan>ὑπομνημ<ex>ατισθεῖσι</ex></expan> ἐπὶ τῆ<supplied reason="lost">ς κ</supplied>ρατίστης βουλῆς καταπάτησιν ποιήσασθαι χωρίων 

    <lb n="7"/>καὶ παραδείσων καὶ <supplied reason="lost">ἀ</supplied>ναγραφὴν φυτῶν τῶν καταλαμβανομέν<supplied reason="lost">ων δη</supplied>λῶσαι <choice><reg>ὑμῖν</reg><orig><hi rend="diaeresis">ὑ</hi>μεῖν</orig></choice> 

    <lb n="8"/>καὶ διάθεσιν τῶν <expan><ex>πρότερον</ex></expan> <gap reason="lost" quantity="2" unit="character"/>το<gap reason="lost" quantity="2" unit="character"/><unclear>λ</unclear>ομης νυνὶ τοῦ ἁγιωτάτου Σαραπίου <app type="editorial"><lem resp="BL cf. 1.412"><hi rend="diaeresis">ἱ</hi>ερ<supplied reason="lost">α</supplied><unclear>τικ</unclear><supplied reason="lost">οῦ</supplied></lem><rdg><hi rend="diaeresis">ἱ</hi>ερ<supplied reason="lost">ε</supplied>ί<unclear>ων</unclear></rdg></app> λόγου 

    <lb n="9"/>ὧν ἐστιν μισθω<supplied reason="lost">τὴς</supplied> <expan>Αὐρήλ<ex>ιος</ex></expan> Πα<supplied reason="lost">ν</supplied>ίσκος ὁ καὶ Μέλας <expan>γυ<ex>μνασιαρχ  </ex></expan> καὶ <expan>πρυτ<ex>αν  </ex></expan> καὶ ὡς <expan>χρη<ex>ματίζει</ex></expan> <expan>δηλ<ex>οῦμεν</ex></expan> <choice><reg>ὑμῖν</reg><orig>ὑμεῖν</orig></choice> <g rend="extension" type="filler"/> 

    <lb n="10"/>ὡς ἑξῆς <hi rend="diaeresis">ὑ</hi>πόκειται. <space extent="unknown" unit="character"/> ἔστι δέ· <space extent="unknown" unit="character"/> 

    <lb n="11"/>ἐν κώμῃ Βώου γενά<supplied reason="lost">μ</supplied>ενοι συν<supplied reason="lost">ό</supplied>ντος ἡμῖν καὶ τοῦ ἀξιολογωτάτου ἐνάρχου πρυτάνεως 

    <lb n="12"/>Αὐρηλίου Ἀσκληπιάδου ἐπα<supplied reason="lost">κ</supplied>ολουθοῦντος Μα<unclear>ύρο</unclear>υ φροντιστοῦ τῆς οὐσίας ἐπείδομεν 

    <lb n="13"/>τῶν ἐκ τοῦ Κ<supplied reason="lost">α</supplied>λλιμάχου κλήρου <expan><supplied reason="lost">ἀμ</supplied>π<supplied reason="lost">ε</supplied>λικ<ex>ὸν</ex></expan> χωρίον <expan><ex>ἀρουρῶν</ex></expan> <num value="22">κβ</num> ἢ ὅσων <supplied reason="omitted">ἐὰν ὦσιν</supplied>, ὀλίγην μὲν ἄμπε

    <lb n="14" break="no"/>λον ζωφυτοῦσ<supplied reason="lost">α</supplied>ν καὶ <supplied reason="lost">τ</supplied>αύτην ἐν <gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ωτ<unclear>άτ</unclear>ῳ ἀμελίᾳ οὖσαν καὶ ἔνθρυον, κύκλῳ δὲ τοῦ 

    <lb n="15"/>χωρίου χέρσον πολλὴν καὶ θρύ<supplied reason="lost">ον</supplied>, τὸν δὲ λάκκον καὶ τὸ <choice><reg>ὑδροδοχεῖον</reg><orig><hi rend="diaeresis">ὑ</hi>δροδοχῖον</orig></choice> <choice><reg>τὸ</reg><orig>τοὺ</orig></choice> <choice><reg>ἐκ</reg><orig>ἐγ</orig></choice> νότου 

    <lb n="16"/>τοῦ χωρίου ἐ<unclear>ν</unclear> συμπτώσει ἀπὸ <unclear>μ</unclear><supplied reason="lost">έρ</supplied>ους· ἐπέ<supplied reason="lost">κει</supplied>το δὲ τῷ ὀργάνῳ μυξέα ζωφυτοῦσα 

    <lb n="17"/>καὶ <choice><reg>συκάμιναι</reg><orig>συκάμειναι</orig></choice>  <num value="20">κ</num> συνῃ<supplied reason="lost">ρη</supplied>μέν<supplied reason="lost">αι κ</supplied>αὶ ἄλλαι ἐσκορπισμέναι ἐν τῷ χωρίῳ <choice><reg>βραχεῖαι</reg><orig>βραχῖαι</orig></choice> μύξαι 

    <lb n="18"/>ἀριθμῷ <num value="60">ξ</num> καὶ πρὸς <supplied reason="omitted">τῷ</supplied> νοτίνῳ <choice><reg>τείχ<supplied reason="lost">ει</supplied></reg><orig>τίχ<supplied reason="lost">ει</supplied></orig></choice> <supplied reason="lost">τ</supplied>οῦ κτήματος ἐν τῷ ἀπὸ νότου πρὸς τῷ λιβὶ λη

    <lb n="19" break="no"/>νῶνι σὺν πίθῳ ὁμοίως ἐν <supplied reason="lost">συμπ</supplied>τ<supplied reason="lost">ώσ</supplied>ει, τῷ δὲ βορινῷ ὀργάνῳ καὶ <supplied reason="lost">λάκ</supplied>κῳ ἐν 

    <lb n="20"/>συμπτώσει ἀπὸ μέρους καὶ αὐτο<gap reason="lost" quantity="9" unit="character"/>ε<supplied reason="lost">ν</supplied>ην ζωφυτοῦσαν μυξε<gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐγ</supplied>

    <lb n="21" break="no"/>γύθεν τοῦ ὀργάνου οἰ<supplied reason="lost">κί</supplied>α παλαιὰ <gap reason="lost" quantity="9" unit="character"/>κ<gap reason="lost" quantity="3" unit="character"/> σὺ<unclear>ν</unclear> <app type="editorial"><lem resp="BL cf. 1.412"><supplied reason="lost">λ</supplied>ην<supplied reason="lost">ῷ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> μετὰ ταύτην τὴν</supplied></lem><rdg><supplied reason="lost">λ</supplied>ην<supplied reason="lost">ῷ </supplied><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="22"/>ἐπιθεώρησιν γενάμενοι <unclear>συν</unclear><supplied reason="lost">όντος </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> <app type="editorial"><lem resp="BL cf. 1.412">ἐπείδαμεν</lem><rdg/></app> ἐκ τοῦ</supplied> 

    <lb n="23"/>Φιλίσκου κλήρου φοινικοπαράδ<supplied reason="lost">εισον </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="24"/>μεν ἀπηλιωτικῷ μέρει τὸ ὑπάρ<supplied reason="lost">χον </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="25"/>τὸ δὲ ἄλλο μέρος πρ<supplied reason="lost">ὸς τῷ</supplied> λι<unclear>β</unclear><supplied reason="lost">ὶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="26"/>θα σῶμα μέγα περ<supplied reason="lost">σέας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="27"/>περ<supplied reason="lost">σέ</supplied>αν ζωφυτ<supplied reason="lost">οῦσαν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="28"/>καὶ μ<supplied reason="lost">ετὰ το</supplied>ύτου παρ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="29"/><gap reason="lost" quantity="6" unit="character"/>α χέρσῳ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="30"/><gap reason="lost" extent="unknown" unit="line"/> 
</ab></div></div>
