         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="FrA,3" type="textpart"><ab>

    <lb n="1"/><app type="editorial"><lem resp="BL cf. 1.412">ἀπὸ λι<supplied reason="lost">βὸς</supplied></lem><rdg>ἀπολ<gap reason="illegible" quantity="4" unit="character"/></rdg></app> <gap reason="illegible" quantity="2" unit="character"/><unclear>ι</unclear><gap reason="illegible" quantity="1" unit="character"/>ους με<gap reason="lost" extent="unknown" unit="character"/>τι καὶ <expan>ἀπηλ<ex>ιώτῃ</ex></expan> ἄχρι κε<unclear>θ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><expan>ἀπηλ<ex>ιώτου</ex></expan> ἄλλη <num value="1">α</num> καὶ ἐν τῷ λοι<unclear>π</unclear><supplied reason="lost">ῷ </supplied><gap reason="lost" extent="unknown" unit="character"/>ιν φοίνικες ἄλ<supplied reason="lost">λα</supplied>ι <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><expan>γ<ex>ίνονται</ex></expan> ἐπὶ τὸ αὐτὸ συνεστῶτες φ<supplied reason="lost">οίνικες</supplied> ἀριθμῷ <num value="178">ροη</num> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>ἐκ<supplied reason="lost">κε</supplied>κομμένη <num value="1">α</num>· γενάμ<supplied reason="lost">ενοι δὲ ε</supplied>ἰς ἕτερον χω<supplied reason="lost">ρίον </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>ἐπεθεωρήσαμεν αὐτὸ μὲν <supplied reason="lost">κείμενον ὅ</supplied>λον ἐν χέρσῳ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>καὶ ἄχρηστον ἐν τῷ κτήμα<supplied reason="lost">τι </supplied><gap reason="lost" quantity="12" unit="character"/>μω αὐτο<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>μεν περσέαν μεγάλην ἐκ<supplied reason="lost">κεκομμένην <num value="1">α</num></supplied> καὶ μ<gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ζω</supplied>

    <lb n="8" break="no"/>φυτούσας δύο <num value="2"/> καὶ σώματα μ<supplied reason="lost">εγάλα </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>ματι τὸ δὲ ἕτ<supplied reason="lost">ερον</supplied> ἓν <num value="1"/> διὰ του<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>αὐτοῦ γεουχῶν μετα φ<gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐσκορ</supplied>

    <lb n="11" break="no"/>πισμένας φοίνικας <supplied reason="lost">τε</supplied>σσ<supplied reason="lost">αρ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>συνεστηκότο<supplied reason="lost">ς</supplied> κ<supplied reason="lost">α</supplied>ὶ ἐγνωσθη<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>περ<supplied reason="lost">σέ</supplied><hi rend="diaeresis">ϊ</hi>νος κείμ<supplied reason="lost">εν</supplied>ος ὡς π<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/>επει παρ<gap reason="lost" quantity="4" unit="character"/>η <supplied reason="lost">ὁμ</supplied>ολογῶ τα<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/>ην <abbr>ἀμπελ<supplied reason="lost">ιτι</supplied>κ</abbr> ἀ<gap reason="lost" quantity="5" unit="character"/>υτη<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>ης καὶ ἐσ<unclear>π</unclear><supplied reason="lost">αρμέ</supplied><unclear>ν</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/>τοις<gap reason="lost" extent="unknown" unit="character"/> 

