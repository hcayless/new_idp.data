<ab>
    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/>

    <lb n="1"/>ἀ<unclear>γῶν</unclear><supplied reason="lost">ος</supplied> ἰ<unclear>σο</unclear><supplied reason="lost">λυμπίου ἐν Γάζῃ τῶν ἀπὸ</supplied>

    <lb n="2"/>Φαμενὼθ <supplied reason="lost">τοῦ <expan>διελ<ex>ηλυθότος</ex></expan> <num value="14">ιδ</num> <expan><ex>ἔτους</ex></expan> ἕως Χοιὰκ</supplied>

    <lb n="3"/>καὶ αὐτοῦ τοῦ <supplied reason="lost">ἐνεστῶτος <num value="15">ιε</num> <expan><ex>ἔτους</ex></expan> μηνῶν</supplied>

    <lb n="4"/><num value="10">ι</num><g type="slanting-stroke"/> ὡς τοῦ μ<supplied reason="lost">ηνὸς <expan><ex>δραχμῶν</ex></expan> <num value="200">σ</num> τὰς συναγομέ</supplied>

    <lb n="5" break="no"/>νας <expan>ἀργ<ex>υρίου</ex></expan> δρα<supplied reason="lost">χμὰς <choice><reg>δισχιλίας</reg><orig>δισχειλίας</orig></choice></supplied> <num value="2000"/>

    <lb n="6"/><expan><ex>γίνονται</ex></expan> <expan>ἀργ<ex>υρίου</ex></expan> <expan><ex>δραχμαὶ</ex></expan> <num value="2000">Β</num>· <supplied reason="lost"><expan><ex>ἔτους</ex></expan> <num value="15">ιε</num></supplied>

    <lb n="7"/>Αὐτοκράτορο<supplied reason="lost">ς Καίσαρος Πουπλίου</supplied>

    <lb n="8"/>Λικιννίου <supplied reason="lost">Γαλλιηνοῦ Γερμανικοῦ</supplied>

    <lb n="9"/>Μεγί<supplied reason="lost">σ</supplied><unclear>τ</unclear>ου Π<unclear>ε</unclear>ρσ<unclear>ι</unclear>κοῦ <unclear>Μ</unclear><supplied reason="lost">εγίστου</supplied>

    <lb n="10"/>Εὐσεβοῦς Εὐτυχοῦς <supplied reason="lost">Σεβαστοῦ</supplied>

    <lb n="11"/>Μεχείρ. <handShift new="m2"/>Αὐρήλιος Σε<unclear>ρ</unclear><supplied reason="lost">ῆνος</supplied> <choice><reg><supplied reason="lost">ἱερονί</supplied><lb n="12" break="no"/>κης</reg><orig><supplied reason="lost">ἱαιρονεί</supplied>

