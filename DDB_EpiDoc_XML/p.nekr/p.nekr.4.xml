    <lb n="14"/>Γῆν Ἥλιον ἀπὸ τοῦ ν<unclear>ῦ</unclear><supplied reason="lost">ν μέχρι παντὸς χάριν ἧς ἐνέ</supplied>

    <lb n="15" break="no"/>δειξας πρὸς ἡμᾶς πί<unclear>σ</unclear><supplied reason="lost">τεως</supplied> <gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">καὶ</supplied>

    <lb n="16"/>μὴ ἐξεῖναι <choice><reg>ἡμῖν</reg><orig>ἡμεῖν</orig></choice> <choice><reg>ἐγ<supplied reason="lost" cert="low">καλεῖν</supplied></reg><orig>ἐν<supplied reason="lost" cert="low">καλεῖν</supplied></orig></choice> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="17"/>π<supplied reason="lost">ερ</supplied><unclear>ὶ</unclear> <unclear>δο</unclear>υλίας ἀπὸ τοῦ <supplied reason="lost">νῦν μέχρι παντὸς</supplied> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="18"/>μ<unclear>η</unclear><gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">μετελθεῖν</supplied>

    <lb n="19"/>σε περὶ δουλίας ἢ ὑπ<unclear>ὲ</unclear><supplied reason="lost">ρ</supplied> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="20"/>διὰ τὸ οὕτως <choice><reg>ἡμῖν</reg><orig>ἡμεῖν</orig></choice> <supplied reason="lost">δεδόχθαι, καὶ δεδωκέναι</supplied>

    <lb n="21"/>σοι μέρος τρίτον οἰ<unclear>κ</unclear><supplied reason="lost">ίας</supplied> <gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">γεί</supplied>

    <lb n="22" break="no"/>τονες νότου ὁδὸς μ<gap reason="lost" extent="unknown" unit="character"/>

