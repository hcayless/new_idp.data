    <lb n="24" break="no"/><gap reason="lost" quantity="18" unit="character" precision="low"/> τοῖς <gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="7" unit="character"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="9" unit="character"/><supplied reason="lost"> τ</supplied>ῷ Πετ<supplied reason="lost">οσ</supplied>ί<unclear>ρ</unclear>ι <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/>

    <lb n="25"/><gap reason="lost" quantity="16" unit="character" precision="low"/> <supplied reason="lost">Τι</supplied>μούθι<supplied reason="lost">ος</supplied> <gap reason="lost" quantity="3" unit="character"/> <unclear>ἐ</unclear>μαυτ<gap reason="lost" quantity="3" unit="character"/> <gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="3" unit="character"/>νω<gap reason="illegible" quantity="1" unit="character"/>α<gap reason="illegible" quantity="1" unit="character"/><unclear>ισ</unclear>ο <unclear>Μ</unclear>αρια<unclear>νο</unclear><gap reason="lost" quantity="1" unit="character"/>

    <lb n="26"/><gap reason="lost" quantity="18" unit="character" precision="low"/><gap reason="illegible" quantity="1" unit="character"/>αι <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="3" unit="character"/><unclear>μ</unclear>εν ἀπ<supplied reason="lost">ὸ</supplied> θυγατρὸς Λυκοδώρας Νεμε

    <lb n="27" break="no"/><supplied reason="lost" cert="low">σίωνος </supplied> <gap reason="lost" quantity="7" unit="character" precision="low"/> <supplied reason="lost">δραχμ</supplied><unclear>ὰ</unclear>ς <num value="900"><choice><reg>ἐν<supplied reason="lost">α</supplied>κοσίας</reg><orig>ἐν<unclear>ν</unclear><supplied reason="lost">εα</supplied><unclear>κ</unclear>οσίας</orig></choice></num> σ<gap reason="illegible" quantity="2" unit="character"/>μ<gap reason="illegible" quantity="2" unit="character"/> ἅσπερ ἀπεσχηκέναι

    <lb n="28"/><gap reason="lost" quantity="18" unit="character" precision="low"/> μετὰ <supplied reason="lost">κυ</supplied><unclear>ρ</unclear>ίου Ψεννήσιος Ἀμμωνίου ἀνδρὸς

