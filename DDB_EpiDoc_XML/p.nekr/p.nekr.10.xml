    <lb n="31" break="no"/><supplied reason="lost">ματος</supplied> <gap reason="lost" quantity="13" unit="character" precision="low"/><gap reason="illegible" quantity="1" unit="character"/> Πμο<supplied reason="lost">υν</supplied> <gap reason="lost" quantity="14" unit="character" precision="low"/><gap reason="illegible" quantity="3" unit="character"/> <unclear>εν</unclear><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>αε</unclear>ως

    <lb n="32"/><gap reason="lost" quantity="18" unit="character" precision="low"/><unclear>ι</unclear> ἐν το<unclear>ῖ</unclear><supplied reason="lost">ς</supplied> <gap reason="lost" quantity="17" unit="character" precision="low"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="2" unit="character"/><unclear>σιας</unclear> ἐ<unclear>ν</unclear> πᾶσι

    <lb n="33"/><gap reason="lost" quantity="18" unit="character" precision="low"/>απις ασ<gap reason="lost" quantity="21" unit="character" precision="low"/> <unclear>μιᾶς</unclear> <gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="3" unit="character"/>

    <lb n="34"/><gap reason="lost" quantity="18" unit="character" precision="low"/>ε<gap reason="lost" quantity="1" unit="character"/>υ<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/>

