<lb n="2" xml:id="lb2"/><gap reason="lost" quantity="8" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ις <expan><ex>ὁμοίως</ex></expan>
<lb n="3" xml:id="lb3"/><gap reason="lost" quantity="6" unit="character"/><abbr><gap reason="illegible" quantity="1" unit="character"/></abbr> Α<gap reason="illegible" quantity="5" unit="character"/>μι <expan>κερ<ex>άμια</ex></expan> <num value="18">ιη</num>
</ab>
