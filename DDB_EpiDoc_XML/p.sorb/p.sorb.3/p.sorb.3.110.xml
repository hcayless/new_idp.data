      <body>
         <head n="2602" xml:lang="en">
            <date>220/219 BC</date>
            <placeName>Mouchis</placeName>
            <ref n="sb;18;13839" type="reprint-from">SB 18 13839</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>βασιλεῖ Πτολεμαίωι χαίρειν <supplied reason="lost">Ταύτως </supplied><gap reason="lost" quantity="20" unit="character" precision="low"/> 

    <lb n="2"/>τῆς Πολέμωνος μερίδος. ἀδι<unclear>κ</unclear><supplied reason="lost">οῦμαι ὑπὸ Δημητρίου </supplied><gap reason="lost" quantity="10" unit="character" precision="low"/> 

    <lb n="3"/>Ἀλκιμάχου τῶν ἐκ τῆς αὐτῆς <unclear>κ</unclear><supplied reason="lost">ώμης </supplied><gap reason="lost" quantity="23" unit="character" precision="low"/> 

    <lb n="4"/>Στρατονίκης τελευτησάσ<supplied reason="lost">ης </supplied><gap reason="lost" quantity="25" unit="character" precision="low"/> 

    <lb n="5"/>ἀνέπεισέν με τοῦ <num value="3">γ</num> <expan><ex>ἔτους</ex></expan> Ἐπ<unclear>εὶφ</unclear> <gap reason="lost" quantity="27" unit="character" precision="low"/> 

    <lb n="6"/>αὐ<unclear>τοὺ</unclear>ς ἐπὶ τοῦ τάφου, ἐμοῦ δ<supplied reason="lost">ὲ </supplied><gap reason="lost" quantity="26" unit="character" precision="low"/> 

    <lb n="7"/>Δημήτριος καταφρονήσας <unclear>μ</unclear><supplied reason="lost">ου ὅτι </supplied><gap reason="lost" quantity="22" unit="character" precision="low"/> 

    <lb n="8"/>παραλαβὼν αὐτοῦ τὸν ἀδελφὸν <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="27" unit="character" precision="low"/> 

    <lb n="9"/><unclear>π</unclear>αι καὶ ἀπηνέγκατο πρὸς ἑαυ<supplied reason="lost">τὸν </supplied><gap reason="lost" quantity="24" unit="character" precision="low"/> 

    <lb n="10"/>δέομαι οὖν σου, βασιλεῦ, μὴ περ<supplied reason="lost">ιιδεῖν με ὑπὸ </supplied><gap reason="lost" quantity="16" unit="character" precision="low"/> 

    <lb n="11"/><supplied reason="lost">ἀδικ</supplied>ουμένην ἀλλὰ προστάξα<supplied reason="lost">ι Διοφάνει τῶι στρατηγῶι γράψαι</supplied> 

    <lb n="12"/><supplied reason="lost">Δη</supplied><unclear>μ</unclear>ητρίωι τῶι ἐπιστάτηι ἀπ<unclear>ο</unclear><supplied reason="lost">στεῖλαι αὐτὸν ἐπὶ Διοφάνην καὶ</supplied> 

    <lb n="13"/><supplied reason="lost">ἐὰν ἦι ἃ γράφω</supplied> ἀληθῆ, ἐπαν<unclear>α</unclear><supplied reason="lost">γκάσαι αὐτὸν </supplied><gap reason="lost" quantity="15" unit="character" precision="low"/> 

    <lb n="14"/>τ<unclear>ὰ</unclear> <unclear>ὑπ</unclear>ογεγ<supplied reason="lost">ρα</supplied><unclear>μ</unclear>μέ<unclear>να</unclear> <unclear>ἢ</unclear> τ<unclear>ὴν</unclear> <unclear>δ</unclear><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="10" unit="character"/><supplied reason="lost"> τούτου γὰρ γενομένου</supplied> 

    <lb n="15"/>ἔσομαι ἐπὶ σὲ καταφυγοῦσα, βασιλ<supplied reason="lost">εῦ, τὸν πάντων εὐεργέτην, τοῦ δικαίου</supplied> 

    <lb n="16"/>τετευχυῖα. <space extent="unknown" unit="character"/> 

    <lb n="17"/><supplied reason="lost">εὐτύχει.</supplied>

<lb n="18"/>ἔστιν δὲ τὸ καθʼ ἕν <num value="1"/>· χιτὼν ἄνθινος <gap reason="lost" quantity="20" unit="character" precision="low"/><supplied reason="lost"> ἐνωτίων</supplied> 

    <lb n="19"/>χρυσῶν ζεύγ<unclear>η</unclear> δύο <num value="2"/> ἄξια <expan><ex>δραχμῶν</ex></expan> <num value="24">κδ̅</num> γρ<unclear>ύ</unclear><supplied reason="lost">τη </supplied><gap reason="lost" quantity="25" unit="character" precision="low"/> 

    <lb n="20"/>ἀλάβαστροι <num value="4">δ</num> <expan><ex>δραχμῶν</ex></expan> <num value="8">η</num> καὶ <choice><reg cert="low">ἑτέρα</reg><orig>ἑτέραν</orig></choice> <choice><reg cert="low">γρύτ<supplied reason="lost">η</supplied></reg><orig>γρύτ<supplied reason="lost">ην</supplied></orig></choice> <gap reason="lost" quantity="25" unit="character" precision="low"/> 


    <lb n="21"/><handShift new="m2"/> Δημητρίωι· μάλιστ<supplied reason="lost">α μὲν διάλυσον αὐτούς· εἰ δὲ μή, ἀπόστει</supplied>

    <lb n="22" break="no"/>λον πρὸς ἡμᾶς ὅπως <supplied reason="lost">ἐπὶ τοῦ καθήκοντος κριτηρίου δια</supplied>

    <lb n="23" break="no"/>κριθῶσιν. <supplied reason="lost"><expan><ex>ἔτους</ex></expan> </supplied><gap reason="lost" quantity="3" unit="character"/>
</ab></div>
