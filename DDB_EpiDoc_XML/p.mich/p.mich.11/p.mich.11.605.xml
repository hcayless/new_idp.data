         <head n="12282" xml:lang="en">
            <date>AD 117</date>
            <placeName>Bacchias</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><gap reason="illegible" quantity="1" unit="line"/> 

    <lb n="2"/><gap reason="lost" quantity="30" unit="character"/><supplied reason="lost"> οὐ</supplied><unclear>λὴ</unclear> <unclear>ὑπ</unclear><gap reason="lost" quantity="6" unit="character"/> Ἑρμᾷ Πτολεμ<unclear>αίου</unclear> 

    <lb n="3"/><gap reason="lost" quantity="10" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="8" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><choice><reg>τοὺς</reg><orig>τον</orig></choice> μὲ<unclear>ν</unclear> Κατοίτην καὶ Πετεσοῦχον <choice><reg>ἔχειν</reg><orig>ἔχιν</orig></choice> 

    <lb n="4"/>παρ<unclear>ὰ</unclear> <supplied reason="lost">το</supplied>ῦ <unclear>Ἑ</unclear>ρμᾶ ἀργυρίου ἐ<unclear>π</unclear><supplied reason="lost">ι</supplied><unclear>σ</unclear>ήμου Σεβαστῶν νομίσματος κεφαλαίου δραχμὰ<supplied reason="lost">ς</supplied> 

    <lb n="5"/><num value="60">ἑξήκοντα</num> παραχρῆμα διὰ <unclear>χειρὸ</unclear>ς ἐξ οἴκου καὶ ἀντὶ τῶν τούτων τόκων 

    <lb n="6"/><choice><reg>συγκεχωρηκέναι</reg><orig>συνκεχωρηκέναι</orig></choice> τοὺς ὁμολ<unclear>ογοῦ</unclear>ντας τῶι Ἑρμᾷ <choice><reg>ἐνοικεῖν</reg><orig>ἐνοικῖν</orig></choice> αὐτὸν καὶ τοὺς 

    <lb n="7"/>παρʼ αὐτοῦ καὶ οὓς ἐὰν βού<unclear>λη</unclear>ται ἐν τῶι ὑπάρχοντι αὐτοῖς τοῖς ὁμολογοῦσι 

    <lb n="8"/>ἐν τῇ προγεγραμμένῃ κ<unclear>ώμ</unclear>ῃ π<unclear>α</unclear>τ<unclear>ρ</unclear>ικῶι ἐνάτῳ καὶ <subst><add place="inline"><choice><reg>εἰκοσιεβδόμῳ</reg><orig>εἰκοσεβδομωι</orig></choice></add><del rend="corrected">ιικοσεβδομωι</del></subst> μέρ

    <lb n="9" break="no"/>ει κοινῶι καὶ ἀδιαιρέτ<unclear>ῳ</unclear> <unclear>π</unclear>ρὸς αὐτὸν Ἑρμᾶν τῶν λοιπῶν μερῶν οἰκοπέ

    <lb n="10" break="no"/>δων καὶ τῶν <choice><reg>συγκυρόντων</reg><orig>συνκυρόντω<unclear>ν</unclear></orig></choice> πάντων καὶ <choice><reg>βεβαιώσειν</reg><orig>βεβαιώσιν</orig></choice> τοὺς ὁμολογοῦντας καὶ 

    <lb n="11"/>τοὺς παρʼ αὐτῶν τῷ Ἑρμᾷ <supplied reason="lost">κ</supplied><unclear>α</unclear>ὶ τοῖς παρʼ αὐτοῦ τὰ κατὰ τὴν ἐνοίκησιν <expan>ταύτη<ex>ν</ex></expan> 

    <lb n="12"/>πάσῃ <choice><reg>βεβαιώσει</reg><orig>βεβαιώσι</orig></choice> καὶ <choice><reg>παρεχ<supplied reason="lost">έ</supplied>σθωσαν</reg><orig>παρεχ<supplied reason="lost">έ</supplied><unclear>σ</unclear>θωισαν</orig></choice> καθαρὰ ἀπὸ παντὸς <choice><reg>ὀφειλήματος</reg><orig>ὀφιλήματος</orig></choice> <expan>δημοσί<ex>ου</ex></expan> 

    <lb n="13"/>τε καὶ ἰδιωτικοῦ καὶ μηδέ<unclear>να</unclear> <choice><reg>ἐμποιούμενον</reg><orig>ἐνποιούμενον</orig></choice> αὐτῶν μηδὲ μέρους μηδὲ <choice><reg><expan>κωλύοντ<ex>α</ex></expan></reg><orig><expan>κολύοντ<ex>α</ex></expan></orig></choice> <supplied reason="omitted"> </supplied> 

    <lb n="14"/>ἐὰν δὲ φανῇ ὁ Ἑ<unclear>ρ</unclear>μᾶς ἀνηλ<unclear>ω</unclear>κὼς δαπάνην ἐπισκευῆς ἢ ἀνοικοδομῆς καὶ 

    <lb n="15"/>βούλωνται οἱ ὁμολογοῦντες <choice><reg>ἀποδοῦναι</reg><orig>ἀ<unclear>πο</unclear>δῶναι</orig></choice> ἀποδότωσαν σὺν καὶ τῷ <choice><reg>προκειμένῳ</reg><orig>προκιμένῳ</orig></choice> <expan>ἀργυρί<ex>ῳ</ex></expan>, 

    <lb n="16"/>τῆς πίστεως περὶ αὐτὸν <unclear>Ἑ</unclear><supplied reason="lost">ρ</supplied>μᾶν οὔσης. <expan>ὑπο<ex>γραφεὺς</ex></expan> <expan>τῶ<ex>ν</ex></expan> <expan>ὁμολ<ex>ογούντων</ex></expan> <handShift new="m2"/> <choice><reg><expan>Ἡλιόδ<ex>ωρος</ex></expan></reg><orig><expan>Ἡλειόδω<ex>ρος</ex></expan></orig></choice> <choice><reg><expan>Ἡλιοδ<ex>ώρου</ex></expan></reg><orig><expan>Ἡλει<unclear>ο</unclear>δ<ex>ώρου</ex></expan></orig></choice> <unclear>ὡς</unclear> <expan><ex cert="low">ἐτῶν</ex></expan> <num value="44">μδ</num> <expan><unclear>ο</unclear>ὐ<ex>λὴ</ex></expan> <expan><unclear>μή</unclear><ex>λῳ</ex></expan> <expan><unclear>δ</unclear><ex>εξιῷ</ex></expan>. 

    <lb n="17"/>Κα<unclear>το</unclear>ίτης καὶ <choice><reg>Πετεσοῦχος</reg><orig>Πεχου</orig></choice> ἀ<unclear>μφό</unclear>τεροι Ὥρου καὶ ἡ μήτηρ τούτων <gap reason="lost" quantity="4" unit="character"/><unclear>ι</unclear>ος 

    <lb n="18"/>Κατοίτου μετὰ κυρίου τοῦ τῆς θυγατρὸς αὐτῆς <app type="editorial"><lem resp="P. Heilporn (michigan.apis.1327)">Σουήριος</lem><rdg>Θα<unclear>υ</unclear>ήρ<unclear>ιος</unclear></rdg></app> ἀνδρὸς

    <lb n="19"/>Ἀπολλωνίου τοῦ Ἀπολλων<supplied reason="lost">ί</supplied>ου ὁμολογοῦμεν ἔχ<unclear>ε</unclear>ιν παρὰ τοῦ Ἑρμᾶ 

    <lb n="20"/>τὰς τοῦ ἀργυρίου δραχμ<unclear>ὰς</unclear> <num value="60">ἑξήκον<supplied reason="omitted">τα</supplied></num> διὰ χειρὸς καὶ ἀντὶ τῶν τούτων τόκων 

    <lb n="21"/>συνχωροῦμεν <choice><reg>ἐνοικεῖν</reg><orig>ἐνοικῖν</orig></choice> αὐτὸν ἐν τῶι ἐπιβάλλοντι ἡμῖν 

    <lb n="22"/>ἐνάτωι καὶ <choice><reg>εἰκοσιεβδόμῳ</reg><orig>εἰκοσιε<unclear>β</unclear>δόμου</orig></choice> <choice><reg>μέρει</reg><orig>μέρι</orig></choice> <choice><reg>τόπων</reg><orig>τόπον</orig></choice> οἰκοπέδων 

    <lb n="23"/>καὶ βεβαιώσο<unclear>μ</unclear>εν <supplied reason="lost">καὶ ἐ</supplied>ὰν φανῇ <choice><reg>ἀνηλωκὼς</reg><orig>ἀνηλοκὸς</orig></choice> δαπάνην ἐπισ

    <lb n="24" break="no"/>κ<supplied reason="omitted">ευ</supplied>ῆς <supplied reason="omitted">ἢ</supplied> ἀνοικοδ<supplied reason="omitted">ομ</supplied>ῆς ἀπο<supplied reason="lost">δώ</supplied>σομεν, τῆς πίστεως περὶ αὐτὸν 

    <lb n="25"/>οὔσης καθὰ <choice><reg>πρόκειται</reg><orig>πρόκιται</orig></choice>. <supplied reason="lost">ἔγρ</supplied>αψεν <expan>ὑπ<ex>ὲρ</ex></expan> <choice><reg>αὐτῶν</reg><orig>αὐτο͂ν</orig></choice> <choice><reg>Ἡλιόδωρος</reg><orig>Ἡλιόδορος</orig></choice> Ἡλιοδώρου 

    <lb n="26"/>μὴ <choice><reg>εἰδότων</reg><orig>εἰδότον</orig></choice> γράμ<supplied reason="lost">μα</supplied>τα. 

    <lb n="27"/><handShift new="m3"/> Ἑρμᾶς Πτολεμαί<supplied reason="lost">ο</supplied><unclear>υ</unclear> <choice><reg>γέγονε</reg><orig>γέγωνε</orig></choice> <supplied reason="omitted">εἴς με</supplied> ἡ ἀποχὴ καθὼς <choice><reg>πρόκειται</reg><orig>πρόκιται</orig></choice>. 

    <lb n="28"/><handShift new="m4"/> <expan>ἐντέτακ<ex>ται</ex></expan> <expan>δι<ex>ὰ</ex></expan> <expan>το<ex>ῦ</ex></expan> ἐν <expan>Βακχ<ex>ιάδι</ex></expan> <expan>γρ<ex>αφείου</ex></expan>. 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="29"/><expan>ὁμολ<ex>ογία</ex></expan> <expan>Κατοίτ<ex>ου</ex></expan> καὶ <expan>ἄλλω<ex>ν</ex></expan> <expan>πρὸ<ex>ς</ex></expan> Ἑρμᾶν 

    <lb n="30"/><expan>ἐνοικ<ex>ήσεως</ex></expan> </ab></div></div>
