    <lb n="16"/>μη<unclear>θ</unclear>ενὶ γράψας ὅτι <unclear>τ</unclear><gap reason="illegible" quantity="1" unit="character"/> 

    <lb n="17"/>πτα ἡμῖν οὐδʼ ἔγνω<unclear>κ</unclear>ε 

    <lb n="18"/>πα<unclear>ρ</unclear>α<unclear>γε</unclear>νόμενος. 
<choice><reg>ἀπεσ<lb n="19" break="no"/>τάλκαμεν</reg><orig>ἀφεσ<lb n="19" break="no"/><unclear>τά</unclear>λ<unclear>κ</unclear>α<unclear>μεν</unclear></orig></choice> <supplied reason="lost">δ</supplied><unclear>έ</unclear> <unclear>σ</unclear><supplied reason="lost">οι</supplied> καὶ τὴν 

