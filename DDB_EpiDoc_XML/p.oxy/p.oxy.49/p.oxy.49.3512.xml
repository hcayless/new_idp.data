    <lb n="30"/><expan>ἐποικ<ex>ίου</ex></expan> <hi rend="diaeresis">Ἱ</hi>π<g type="apostrophe"/>πο<unclear>ν</unclear>άμου <expan>οἴν<ex>ου</ex></expan> <expan>σηκ<ex>ωμάτων</ex></expan> <num value="63">ξγ</num> <g type="stauros"/> </ab></div></div>
