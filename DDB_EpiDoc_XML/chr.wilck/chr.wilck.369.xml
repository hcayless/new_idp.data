         <head xml:lang="en">
            <date>AD 13/4</date>
            <placeName>Oxyrhynchus</placeName>
            <ref n="p.oxy;4;721" type="reprint-in">P.Oxy. 4,721</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
<lb n="1"/>Γαίωι Σεππίῳ Ῥούφωι 

    <lb n="2"/>παρὰ Πολέμωνος τοῦ Τρύφωνος καὶ <supplied reason="lost">Ἀρχελάου </supplied><gap reason="lost" quantity="10" unit="character"/> 

    <lb n="3"/>βουλόμεθα ὠνήσασθαι ἐν τῶι Ὀξυρυγχ<supplied reason="lost">είτηι </supplied><gap reason="lost" quantity="10" unit="character"/><supplied reason="lost"> ἀπὸ</supplied> 

    <lb n="4"/>ὑπολόγου βασιλικῆς <app type="editorial"><lem><unclear>γ</unclear>ῆς</lem><rdg/></app> ἕως τοῦ <space extent="unknown" unit="character"/> <expan><ex>ἔτους</ex></expan> Καίσ<supplied reason="lost">α</supplied>ρ<supplied reason="lost">ος κλήρων ἐ</supplied>

    <lb n="5" break="no"/>πὶ τοῦ <space extent="unknown" unit="character"/> <expan><ex>ἔτους</ex></expan> Καίσαρος <choice><reg>ἀνειλημμένων</reg><orig>ἀνειλλημένων</orig></choice> καὶ ἀφόρ<supplied reason="lost">ω</supplied><unclear>ν</unclear> 

    <lb n="6"/>γεγονότων καὶ κλήρων τῶν ἕως τοῦ <space extent="unknown" unit="character"/> <choice><reg>ἀνειλημμένων</reg><orig>ἀνειλλημένων</orig></choice> 

    <lb n="7"/>καὶ αὐτοῦ <space extent="unknown" unit="character"/> <expan><ex>ἔτους</ex></expan> Καίσαρος <choice><reg>ἀνειλημμένων</reg><orig>ἀνειλλημένων</orig></choice> πλὴν ἱερᾶς εἰς <unclear>κα</unclear><supplied reason="lost" cert="low">ρποὺς</supplied> 

    <lb n="8"/>τοῦ <choice><reg>εἰσιόντος</reg><orig>ἰσιόντος</orig></choice> τετάρτου καὶ τεσσαρακοστοῦ ἔτους Καίσαρος, <supplied reason="lost">ὁ μὲν</supplied> 

    <lb n="9"/>Πολέμων περὶ Θῶσβιν καὶ Τεποῦιν τ<unclear>ῆ</unclear><supplied reason="lost">ς</supplied> <unclear>ἄ</unclear>νω <unclear>τοπ</unclear><supplied reason="lost">α</supplied><unclear>ρχ</unclear><supplied reason="lost">ίας</supplied> 

    <lb n="10"/><expan>ἀρούρ<ex>ας</ex></expan> δεκαπέντε, <expan><ex>γίνεται</ex></expan> <expan>ἄρουρ<ex>αι</ex></expan> <num value="15">ιε</num>, ὁ δὲ Ἀρχέλ<supplied reason="lost">αος περὶ </supplied><gap reason="lost" quantity="7" unit="character"/><supplied reason="lost"> τῆς</supplied> 

    <lb n="11"/>Θμοισεφὼ <expan>τοπαρχ<ex>ίας</ex></expan> <expan>ἀρούρ<ex>ας</ex></expan> τέσσαρες, <expan><ex>γίνεται</ex></expan> <expan>ἄρουρ<ex>αι</ex></expan> <supplied reason="lost"><num value="4">δ</num>, <expan><ex>γίνεται</ex></expan> <expan>ἄρουρ<ex>αι</ex></expan> <num value="19">ιθ</num></supplied>, 

    <lb n="12"/>ἐφʼ ᾧ παραδειχθέντες ταύτας διαγράψ<unclear>ομ</unclear><supplied reason="lost">εν εἰς τὴν ἐπὶ τῶν τό</supplied>

    <lb n="13" break="no"/>πων <supplied reason="lost">δη</supplied>μοσίαν τράπεζαν τ<unclear>ὴν</unclear> <app type="editorial"><lem resp="BL 12.136"><unclear>κ</unclear>ε<unclear>κρ</unclear><supplied reason="lost">ιμένη</supplied></lem><rdg><unclear>κ</unclear>ε<unclear>κε</unclear><supplied reason="lost">λευσμένην</supplied></rdg></app> <supplied reason="lost">τιμὴν ἑκάστης</supplied> 

    <lb n="14"/><expan>ἀρούρ<ex>ας</ex></expan> <supplied reason="lost"><expan>ἀργυ<ex>ρίου</ex></expan> <expan><ex>δραχμὰς</ex></expan></supplied> δεκαδύο <num value="12"/>, ἕξομεν δὲ εἰς τὴν τ<unclear>ού</unclear><supplied reason="lost">των ἀναγωγὴν καὶ κα</supplied>

    <lb n="15" break="no"/><supplied reason="lost">τεργασίαν ἀτέλειαν </supplied><choice><reg><supplied reason="lost">ἔ</supplied>τη</reg><orig><supplied reason="lost">ἔ</supplied>τηι</orig></choice> τρία <num value="3"/> ἀπὸ τοῦ <supplied reason="lost">εἰσιόντος <num value="44">μδ</num> <expan><ex>ἔτους</ex></expan> Καίσαρος</supplied> 

    <lb n="16"/><gap reason="lost" extent="unknown" unit="line"/>
