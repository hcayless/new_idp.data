         <head n="2901" xml:lang="en">
            <date>226 BC</date>
            <placeName>Gurob</placeName>
            <ref type="reprint-from">cfPPetr3,61,A</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="FrI" type="textpart"><ab>

    <lb n="1"/>βασιλεύον<supplied reason="lost">τος Πτολεμαίου τοῦ Πτολεμαίου καὶ</supplied> 

    <lb n="2"/>Ἀρσινόης Θεῶ<supplied reason="lost">ν Ἀδελφῶν <expan><ex>ἔτους</ex></expan> </supplied><gap reason="lost" quantity="2" unit="character"/><supplied reason="lost"> ἐφʼ ἱερέως</supplied> 

    <lb n="3"/>Ἀλεξάνδρου καὶ Θεῶ<supplied reason="lost">ν Ἀδελφῶν καὶ Θεῶν Εὐεργετῶν</supplied>, 

    <lb n="4"/>κανηφόρου Ἀρσινόης φ<supplied reason="lost">ιλαδέλφου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><app type="editorial"><lem>Χαρίτωνος</lem><rdg>Χαρίτονος</rdg></app> μηνὸς Θῶυθ <num value="22">κβ</num> ἐν Π<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>νομοῦ τῆς Πολέμωνος μερ<supplied reason="lost">ίδ</supplied>ος <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>Ὀρσέους καὶ Πετώυς <app type="editorial"><lem>Ὀννώφριο<supplied reason="lost">ς</supplied> καὶ <gap reason="lost" extent="unknown" unit="character"/></lem><rdg>Ὀνν<supplied reason="lost">ώφ</supplied>ριο<supplied reason="lost">ς </supplied><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="8"/>καὶ Στοτοῆτις Ἀμεννέως καὶ Δ<gap reason="lost" extent="unknown" unit="character"/>νν<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>Μανρῆς μέγας Στοτοήτιος καὶ Μανρῆς <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>Μανρέους καὶ Πασῦς <app type="editorial"><lem>Μανρέους</lem><rdg>μανρειους</rdg></app> καὶ Πας<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>Πετεσοῦχος Πάιτος ὄντες <num value="11">ια</num> ὀνηλ<supplied reason="lost">άται </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>τῆς Πολέμωνος μερίδος διεγγύη<supplied reason="lost">σαν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>Ἀσκληπιάδει οἰκονόμωι <add place="above">καὶ Ψενουνει θ<gap reason="lost" extent="unknown" unit="character"/></add> <del rend="erasure">καὶ <app type="editorial"><lem>Μανρεῖ</lem><rdg>Μανρέους</rdg></app></del> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/><add place="above"><app type="editorial"><lem>παρόντος</lem><rdg>παρὰ τοῦ</rdg></app> Ἀριστάρχου τοῦ <app type="editorial"><lem>παρὰ Διονυσοδώρου</lem><rdg>παραγινοντος Θεοδώρωι<certainty match=".." locus="value"/></rdg></app></add> <app type="editorial"><lem><del rend="erasure">Πα<gap reason="illegible" quantity="1" unit="character"/>ει τῶι παρὰ <gap reason="illegible" quantity="2" unit="character"/><unclear>μ</unclear>ουνιος</del></lem><rdg>Πάετει τῶ<del rend="erasure"><gap reason="illegible" extent="unknown" unit="character"><desc>vestiges</desc></gap></del></rdg></app> ἐφʼ ὧι λαβόν<supplied reason="lost">τες </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/>τιμὴν ὑποζυγίων <num value="24">κδ</num> ἑκάστωι <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>σάκκου <expan><ex>δραχμὰς</ex></expan> <num value="24">κδ</num> ὥστε εἶναι <num value="3360">Γτξ</num> <gap reason="lost" extent="unknown" unit="character"/> </ab></div></div>
