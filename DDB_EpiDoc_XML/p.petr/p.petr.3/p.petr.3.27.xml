    <lb n="2"/><del rend="erasure"><gap reason="illegible" quantity="1" unit="line"/></del> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/>της δὲ γ<supplied reason="lost">ε</supplied>γραμμένης μοι ἐπ 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ονος ἐσφρ<supplied reason="lost">αγ</supplied>ίσατο ἐντεύ

    <lb n="5" break="no"/><supplied reason="lost">ξεως</supplied> ὑποτέθεικά σοι <choice><reg>τὸ ἀντίγραφον</reg><orig>τἀντίγραφον</orig></choice> 

    <lb n="6"/><space extent="unknown" unit="character"/> 

    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" quantity="2" unit="character"/>α Ὀρνίτας καὶ Ζᾶγις 
</ab></div><div n="v,2" type="textpart"><ab>

    <lb n="1"/>προση<supplied reason="lost">γ</supplied>γέλθαι κ<gap reason="illegible" quantity="4" unit="character"/>τεσθαι ἐν <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>κατὰ σὲ τόποις. ἐὰν <supplied reason="lost">σ</supplied>υνεπιλαμβάνωσ<supplied reason="lost">ι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>καὶ ὁμολογῶσιν ε<supplied reason="lost">ἶ</supplied>ναι τ<unclear>ῶ</unclear>ν περὶ τὸν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>Ὀρνίταν φορο<unclear>ν</unclear><gap reason="illegible" quantity="2" unit="character"/><unclear>ε</unclear>ον <abbr>πο</abbr> ὑπὸ μηθενὸς <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>ἀποβια<unclear>σ</unclear>θῶσ<supplied reason="lost">ι</supplied>ν ἀλλα <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><unclear>αὐτοῖς</unclear> ἐὰν δέ τι <expan>ἀν<ex>τίγραφον</ex></expan> ἀποσταλῆι ως<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

