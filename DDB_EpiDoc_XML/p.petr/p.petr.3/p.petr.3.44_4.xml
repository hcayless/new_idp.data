         <head n="7452" xml:lang="en">
            <date>240 BC</date>
            <placeName>Gurob</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="Fr4" type="textpart"><ab>

    <lb n="1"/><gap reason="lost" quantity="4" unit="character"/>μαχωι. ἔγραψά σοι <unclear>ὅπ</unclear><supplied reason="lost">ως</supplied> 

    <lb n="2"/><supplied reason="lost">συ</supplied>ντάξηις καλάμ<unclear>ων</unclear> <gap reason="lost" quantity="4" unit="character"/> 

    <lb n="3"/><supplied reason="lost">προσά</supplied>γειν πρὸς τὸ διάλημμα <supplied reason="lost">τὸ</supplied> 

    <lb n="4"/><supplied reason="lost">κατὰ</supplied> Βούσιριν καὶ χωματοφύλα

    <lb n="5" break="no"/><supplied reason="lost">κας</supplied> καταστῆσαι <num value="5">ε</num><gap reason="illegible" quantity="1" unit="character"/>. ὑπὲρ δὲ 

    <lb n="6"/><supplied reason="lost">τούτω</supplied>ν καὶ Ε<unclear>ὐ</unclear>ρώτ<unclear>α</unclear>ι ἔγραψα. οὐθὲν 

    <lb n="7"/><supplied reason="lost">δὲ ὑ</supplied>πακούσαντος ἠνγκάσθην 

    <lb n="8"/><supplied reason="lost">τὴν θ</supplied>ύραν κλείσαι. πόλλου γὰρ <gap reason="illegible" quantity="1" unit="character"/> 

    <lb n="9"/><gap reason="lost" quantity="4" unit="character"/>δέοντος ὕδατος καὶ οὐδέ<gap reason="lost" quantity="4" unit="character"/> 

    <lb n="10"/><gap reason="lost" quantity="4" unit="character"/><unclear>ς</unclear> πρὸς τῆι τοῦ χώματος κ<gap reason="lost" quantity="5" unit="character"/> 

    <lb n="11"/><gap reason="lost" quantity="4" unit="character"/> ὑμῶν οὐδὲ τῶν <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="6" unit="character"/> 

    <lb n="12"/><gap reason="lost" quantity="4" unit="character"/>οντων τὸ ὕδωρ ουθε<gap reason="lost" quantity="6" unit="character"/> 

    <lb n="13"/><gap reason="lost" quantity="4" unit="character"/>εδέχετο ποιεῖν <unclear>ο</unclear>υ<gap reason="lost" quantity="7" unit="character"/> 

    <lb n="14"/><supplied reason="lost">ἀπο</supplied>στεῖλαι εκ<gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>υ<gap reason="lost" quantity="8" unit="character"/> 

    <lb n="15"/><gap reason="lost" quantity="3" unit="character"/>μαι ἀπὸ τῶν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/><gap reason="lost" quantity="4" unit="character"/>οτωι ων <gap reason="lost" extent="unknown" unit="character"/> </ab></div></div>
