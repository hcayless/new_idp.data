    <lb n="17"/>παρὰ σοῦ ἐ<unclear>ν</unclear>τεῦθεν ἤδη 

    <lb n="18"/>εἰς ἀναγκαίας μου χρείας 

    <lb n="19"/>χρυσοῦ νομ<supplied reason="lost">ίσ</supplied>ματα δύο <num value="2"/> 

    <lb n="20"/>καὶ κεράτια τέσσερα <num value="4"/> 

    <lb n="21"/><hi rend="diaeresis">ἰ</hi>διωτικῷ ζυγῷ <expan>νομιτευόμ<ex>ενα</ex></expan>

    <lb n="22"/><expan>γί<ex>νεται</ex></expan> <expan>χρ<ex>υσοῦ</ex></expan> <expan>νο<ex>μίσματα</ex></expan> <num value="2">β</num> καὶ <expan>κερ<ex>άτια</ex></expan> <num value="4">δ</num> <expan><hi rend="diaeresis">ἰ</hi>δ<ex>ιωτικῷ</ex></expan> <expan>ζυγ<ex>ῷ</ex></expan> 

    <lb n="23"/>καὶ ταῦτα ὁμολογῶ παρα<unclear>σ</unclear>χ<add place="above">εῖν</add> 

