         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Κα</supplied>λλινίκου τοῦ <choice><reg>βουκελλαρίου</reg><orig>βουκκελαρίου</orig></choice> ἀντέπεμψα αὐτῇ διὰ τοῦ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> π</supplied>έμψετε καὶ ὡς θέλει <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="illegible" quantity="2" unit="line"/> 

    <lb n="3"/>ἔγραψεν περὶ τοῦ ἑψήματος <gap reason="lost" quantity="5" unit="character"/><supplied reason="lost"> ἀ</supplied>πὸ διατυπώσε<supplied reason="lost">ως </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>κατενεχθῆναι εὐθέως τοὺς τέκτονας κα<supplied reason="lost">ὶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>λόγῳ τῶν καράβων πρὸς τὸν περυσινὸ<supplied reason="lost">ν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>μηνῶν καθὼς καὶ ἐγράφη ὑμῖν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/>ου ἡμῶν λ<gap reason="lost" extent="unknown" unit="character"/> 

