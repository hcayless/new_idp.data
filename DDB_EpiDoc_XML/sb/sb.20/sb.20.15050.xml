    <lb n="7"/><expan>ἐ<ex>πὶ</ex></expan> <expan>λόγ<ex>ου</ex></expan> <expan>ῥυπ<ex>αρὰς</ex></expan> <expan>δραχ<ex>μὰς</ex></expan> τρεῖς τριώβολον, <expan><ex>γίνονται</ex></expan> <expan><ex>δραχμαὶ</ex></expan> <num value="3">γ</num> <num value="3"><expan><ex>τριώβολον</ex></expan></num>.

