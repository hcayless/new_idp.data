    <lb n="25"/><handShift new="m5"/> Πτολεμαῖος <unclear>τε</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/><unclear>χ</unclear>ι κ<unclear>αὶ</unclear> <expan><unclear>ἔγρα</unclear><ex>ψα</ex></expan> 

    <lb n="26"/>χωρὶς ἀλείφα<supplied reason="lost">το</supplied><unclear>ς</unclear> κ<supplied reason="lost">α</supplied><unclear>ὶ</unclear> <expan><unclear>πα</unclear>ρεπιγραφῆ<ex>ς</ex></expan> 

