    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/>Φιβίου <gap reason="lost" quantity="4" unit="character"/>μενου παρ<gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">καὶ</supplied>

