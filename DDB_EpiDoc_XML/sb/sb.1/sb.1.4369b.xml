    <lb n="11" break="no"/><gap reason="lost" extent="unknown" unit="character"/>μωι τὸν δὲ τε

    <lb n="12" break="no"/><gap reason="lost" extent="unknown" unit="character"/>ιν ἤ τι καλῶς <app type="editorial"><lem resp="BL 7.183"><expan>πο<ex>ιήσεις</ex></expan></lem><rdg>ηι</rdg></app> 

    <lb n="13"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Ἀρ</supplied>σινοίτου <app type="editorial"><lem resp="BL 8.309">δωρ<gap reason="illegible" quantity="3" unit="character"/>ς</lem><rdg>δώρῳ τῆς</rdg></app> 

    <lb n="14"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ην ση θ 

    <lb n="15"/><space extent="unknown" unit="character"/> 

    <lb n="15"/><gap reason="lost" extent="unknown" unit="character"/>εν με Ποσειδώνιος 

