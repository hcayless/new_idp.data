    <lb n="7"/>καὶ ἀνοικοδομὴν ποιήσασθ<supplied reason="lost">αι</supplied> <gap reason="lost" quantity="8" unit="character" precision="low"/> <supplied reason="lost">διαγράψαντες</supplied> 

    <lb n="8"/>τὸ ὀφειλόμενον <choice><reg>πρόστιμο<supplied reason="lost">ν</supplied></reg><orig>πρόστειμο<supplied reason="lost">ν</supplied></orig></choice> <gap reason="lost" quantity="21" unit="character" precision="low"/> 

