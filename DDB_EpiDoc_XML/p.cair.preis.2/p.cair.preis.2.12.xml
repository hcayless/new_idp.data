    <lb n="10"/>ἕως <num value="23">κγ</num> <expan><ex>ἔτους</ex></expan> λογισθέντα κα<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="22" unit="character" precision="low"/> 

    <lb n="11"/><space extent="unknown" unit="character"/>

    <lb n="11"/>Φιλαδ<unclear>ε</unclear>λφείας

    <lb n="12"/>Τίτος Φλ<unclear>α</unclear>ύ<hi rend="diaeresis">ι</hi>ος Ἀντω<add place="above">ν</add>ῖνος <gap reason="lost" quantity="22" unit="character" precision="low"/> 

    <lb n="13"/>καὶ ὡς χρηματίζει διὰ φρ<supplied reason="lost">οντιστοῦ</supplied> <gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">ἠξίωσεν</supplied>

    <lb n="14"/>ἐν τοῖς ὑπάρχουσι αὐτῷ σιτικοῖ<supplied reason="lost">ς καὶ ἀμπελικοῖς ὀρύ</supplied>

    <lb n="15" break="no"/>ξαι φρέατ<surplus>ρ</surplus>α <num value="2"><hi rend="supraline">β</hi></num>, <num value="1">ἓν</num> μὲν τοῖς σιτικο<supplied reason="lost">ῖς <expan><ex>ἀρούρης</ex></expan> <num value="1/64"><hi rend="supraline">ξδ</hi></num> <num value="1/128"><hi rend="supraline">ρκη</hi></num>, ἕτερον</supplied>

    <lb n="16"/>δὲ τοῖς ἀμπελικοῖς <expan><ex>ἀρούρης</ex></expan> <num value="1/64"><hi rend="supraline">ξδ</hi></num> <num value="1/128"><hi rend="supraline">ρκη</hi></num>, <expan>γ<ex>ίνεται</ex></expan> <expan><ex>ἄρουρα</ex></expan> <num value="1/32"><hi rend="supraline">λ</hi><supplied reason="lost"><hi rend="supraline">β</hi></supplied></num> <supplied reason="lost"><num value="1/64"><hi rend="supraline">ξδ</hi></num>, πρόστιμον</supplied>

    <lb n="17"/>π<unclear>υ</unclear>ροῦ <expan><ex>ἀρτάβης</ex></expan> <num value="1/4" rend="tick">δ</num>, π<supplied reason="lost">ρ</supplied>οσμετρούμε<unclear>ν</unclear><supplied reason="lost">α <expan><ex>πυροῦ</ex></expan> <expan><ex>ἀρτάβης</ex></expan></supplied> <num><gap reason="lost" atLeast="1" atMost="2" unit="character"/></num>, <supplied reason="lost"><expan><ex>γίνεται</ex></expan> <expan><ex>πυροῦ</ex></expan> <expan><ex>ἀρτάβης</ex></expan></supplied> <num><gap reason="lost" atLeast="1" atMost="2" unit="character"/></num>

    <lb n="17"/><space extent="unknown" unit="character"/> 

