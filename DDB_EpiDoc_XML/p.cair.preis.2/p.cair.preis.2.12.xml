    <lb n="4"/><space extent="unknown" unit="character"/> 

    <lb n="5"/>οἱ ὑπογεγραμμένοι Δίων Ε<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">καὶ</supplied> <gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost"><expan><ex>ἔτους</ex></expan></supplied> <num><gap reason="lost" atLeast="1" atMost="2" unit="character"/></num>

