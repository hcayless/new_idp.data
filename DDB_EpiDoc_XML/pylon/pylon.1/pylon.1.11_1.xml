<ab>
    <lb n="1"/><gap reason="lost" quantity="7" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> τῷ καὶ <hi rend="diaeresis">Ἱ</hi>ππάρχῳ <hi rend="diaeresis">ἱ</hi>ερεῖ ἀρχιδικαστῇ καὶ <unclear>π</unclear><supplied reason="lost">ρὸς τῇ ἐπιμελείᾳ τῶν χρηματισ</supplied>

    <lb n="2" break="no"/><supplied reason="lost">τῶν καὶ τ</supplied><unclear>ῶν</unclear> ἄλλων κριτηρίων. <space extent="unknown" unit="character"/> 

    <lb n="3"/><supplied reason="lost">παρὰ Αὐρ</supplied>ηλίου Ὡριγένους στρ<unclear>ατ</unclear>ιώτου λεγιῶν<unclear>ο</unclear><supplied reason="lost">ς δευτέρας Τραϊανῆς Γερμανικῆς</supplied>

    <lb n="4"/><supplied reason="lost">Ἀντωνινια</supplied>νῆς <hi rend="diaeresis">Ἰ</hi>σχυρᾶς, ἑκατονταρχίας Αὐρηλίου Μα<gap reason="lost" quantity="5" unit="character"/> <supplied reason="lost">καὶ παρὰ Ἰουλίου Εὐτύχου</supplied>

    <lb n="5"/><gap reason="lost" quantity="10" unit="character" precision="low"/> <unclear>Α</unclear>ὐρηλίου Πρωτάρχου ω<gap reason="illegible" quantity="6" unit="character"/><gap reason="lost" quantity="25" unit="character" precision="low"/>

    <lb n="6"/><gap reason="lost" quantity="10" unit="character" precision="low"/><gap reason="illegible" quantity="1" unit="character"/> καὶ Ἀρείου το<gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>ι</unclear>ο<unclear>υ</unclear><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="25" unit="character" precision="low"/>

    <lb n="7"/><supplied reason="lost">συγχωρεῖ ὁ Ἰούλι</supplied><unclear>ο</unclear>ς Εὐτύχης διὰ <supplied reason="lost">συμ</supplied>βεβαιω<unclear>το</unclear>ῦ το<unclear>ῦ</unclear> <supplied reason="lost">Αὐρηλίου Πρωτάρχου πεπρακέναι</supplied>

    <lb n="8"/><supplied reason="lost">τῷ Αὐρηλίῳ Ὡ</supplied><unclear>ριγ</unclear>ένει δού<unclear>λ</unclear><supplied reason="lost">η</supplied><unclear>ν</unclear> ὀνόμα<unclear>τι</unclear> <unclear>Κ</unclear>ην<gap reason="lost" quantity="25" unit="character" precision="low"/>

    <lb n="9"/><gap reason="lost" quantity="7" unit="character" precision="low"/> <supplied reason="lost">γένει Π</supplied>αφλάγαιναν ἀπὸ καταπ<unclear>λόου</unclear> <supplied reason="lost">ἁπλῷ χρήματι καὶ οὖσαν ἐκτὸς ἱερᾶς</supplied>

    <lb n="10"/><supplied reason="lost">νόσου καὶ ἐπα</supplied><unclear>φῆ</unclear>ς, <choice><reg>τιμῆς</reg><orig>τειμῆς</orig></choice> ἀργυρίου δραχμῶν <gap reason="lost" quantity="25" unit="character" precision="low"/>

    <lb n="11"/><supplied reason="lost">ἃς καὶ ἀπεσχη</supplied><unclear>κ</unclear>έναι <choice><reg>τὸν Ἰούλιον Εὐτύχην</reg><orig>ὁ <hi rend="diaeresis">Ἰ</hi>ούλιος Εὐτύχη<unclear>ς</unclear></orig></choice> <unclear>πα</unclear>ρ<unclear>ὰ</unclear> <unclear>τ</unclear><supplied reason="lost">οῦ Αὐρηλίου Ὡριγένους πρὸς τὸ ἀπὸ</supplied>

    <lb n="12"/><supplied reason="lost">τοῦ νῦν τὸν Α</supplied>ὐρήλιον Ὡρ<supplied reason="lost">ι</supplied>γ<supplied reason="lost">έ</supplied><unclear>ν</unclear>ην παρειλη<unclear>φ</unclear><supplied reason="lost">ότα τὴν δούλην Κην– </supplied><gap reason="lost" quantity="10" unit="character" precision="low"/>

    <lb n="13"/><gap reason="lost" quantity="8" unit="character" precision="low"/> <supplied reason="lost">κατὰ</supplied> <unclear>τ</unclear>ὰ τεταγμένα τὴν ἀνάκρισιν  <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="6" unit="character" precision="low"/> <supplied reason="lost">καὶ τεταγμένον ὑπὲρ αὐτῆς τὸ</supplied>

    <lb n="14"/><supplied reason="lost">εἰς τὰ ἀνδράπ</supplied><unclear>ο</unclear>δ<unclear>α</unclear> καθῆκον τέλος κρατεῖν αὐτ<supplied reason="lost">ῆς καὶ κυριεύειν καὶ ἐξουσίαν ἔχειν</supplied>

    <lb n="15"/><supplied reason="lost">ἑτέροις πωλεῖ</supplied>ν καὶ διοικεῖν καὶ ἐπ<unclear>ιτ</unclear>ελεῖν πε<supplied reason="lost">ρὶ αὐτῆς ὡς ἐὰν αἱρῆται ἀνεμποδίσ</supplied>

    <lb n="16" break="no"/><supplied reason="lost">τως, τῆς βεβαι</supplied><unclear>ώ</unclear>σεως ἐξακολουθούσης <unclear>α</unclear>ὐτ<unclear>ῷ</unclear> <supplied reason="lost">τε τῷ ἀποδομένῳ Ἰουλίῳ Εὐτύχῃ</supplied>

    <lb n="17"/><supplied reason="lost">καὶ τῷ βεβαιω</supplied>τῇ Αὐρηλίῳ Πρωτάρχῳ τ<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="25" unit="character" precision="low"/>

    <lb n="18"/><gap reason="lost" quantity="2" unit="character"/> <supplied reason="lost">καὶ πάντα τ</supplied><unclear>ὸ</unclear>ν καθ’ ὁνδη<unclear>π</unclear><supplied reason="lost">ο</supplied>τοῦν τρ<unclear>όπ</unclear><supplied reason="lost">ον ἐπελευσόμενον ἢ ἐμποιησόμε</supplied>

    <lb n="19" break="no"/><supplied reason="lost">νον τῆς δούλη</supplied>ς ἢ μέρους αὐτ<supplied reason="lost">ῆ</supplied><unclear>ς</unclear> χάριν <choice><reg>ἀφισ<supplied reason="lost">τάνειν</supplied></reg><orig>ἀφε<unclear>ισ</unclear><supplied reason="lost">τάνειν</supplied></orig></choice> <supplied reason="lost">παραχρῆμα τοῖς ἰδίοις αὐτῶν</supplied>

    <lb n="20"/><supplied reason="lost">δαπανήμασιν</supplied> ἢ <choice><reg>ἐκτίνειν</reg><orig>ἐκτίννειν</orig></choice> ἐ<unclear>ξ</unclear> <supplied reason="lost">ἀ</supplied>λληλεγγύης <unclear>τ</unclear><supplied reason="lost">ῷ ὠνησαμένῳ τό τε τῆς τιμῆς κεφά</supplied>

    <lb n="21" break="no"/><supplied reason="lost">λαιον σὺν ἡμιολίᾳ καὶ τὰ</supplied> β<unclear>λάβη</unclear> <supplied reason="lost">καὶ δαπανήματα καθάπερ ἐκ δίκης</supplied> <gap reason="lost" extent="unknown" unit="character"/>

