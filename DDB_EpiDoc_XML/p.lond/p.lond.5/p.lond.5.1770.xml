         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/><unclear>ἀ</unclear>πὸ κώ<supplied reason="lost">μης</supplied> <supplied reason="lost">Ἐνσεῦ τοῦ Ἑρμουπολίτου νομοῦ. ὁμολογῶ</supplied> 

    <lb n="2"/>ἐγ<unclear>ὼ</unclear> ὁ προ<unclear>γ</unclear><supplied reason="lost">εγραμ</supplied>μένο<unclear>ς</unclear> <supplied reason="lost">Μην</supplied><unclear>ᾶς</unclear> γεω<unclear>ρ</unclear><supplied reason="lost">γὸς</supplied> <supplied reason="lost">ἑκουσίως</supplied> 

    <lb n="3"/>καὶ αὐθ<unclear>αι</unclear><supplied reason="lost">ρέτω</supplied>ς μεμ<supplied reason="lost">ισ</supplied>θῶσ<unclear>θα</unclear><supplied reason="lost">ι</supplied> <supplied reason="lost">παρʼ ὑμῶν</supplied> 

    <lb n="4"/>ἐπὶ πεν<supplied reason="lost">ταετῆ</supplied> <unclear>χ</unclear>ρόνον λογιζόμε<unclear>ν</unclear><supplied reason="lost">ον </supplied><supplied reason="lost">ἀπὸ καρπῶν τῆς</supplied> 

    <lb n="5"/>σὺν Θεῷ εἰσιούσης<num value="2">δω<unclear>δ</unclear>εκάτης</num> <expan>ἰνδ<ex>ικτίονος</ex></expan> <unclear>τ</unclear><supplied reason="lost">ὴν ὑπάρχουσαν</supplied> 

