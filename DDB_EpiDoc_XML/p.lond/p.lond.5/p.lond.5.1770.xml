    <lb n="7"/>πλέω ἔλαττον ἄν<hi rend="diaeresis">υ</hi>δρον <space extent="unknown" unit="character"/> διακειμ<supplied reason="lost">ένην ἐν <unclear>τόπῳ</unclear></supplied> 

    <lb n="8"/>καλουμένῳ <hi rend="supraline"><unclear>Γ</unclear><gap reason="illegible" quantity="2" unit="character"/><unclear>ξι</unclear>α</hi> ἐν πεδίοις κώ<unclear>μ</unclear><supplied reason="lost">ης </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>εἰς σπορὰν καὶ κατάθεσι<unclear>ν</unclear> καρπῶ<unclear>ν</unclear> <supplied reason="lost">ὧν ἂν αἱρῶμαι</supplied> 

    <lb n="10"/>ἑκάστης ἀρούρης <unclear>γ</unclear>ε<unclear>ωμε</unclear>τρίας φ<supplied reason="lost">όρου</supplied> <supplied reason="lost">καθʼ <unclear>ἕκαστον</unclear></supplied> 

    <lb n="11"/>ἐνιαυτὸν τῷ συμβρόχῳ σίτου ἀρτ<unclear>ά</unclear><supplied reason="lost">β </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> καὶ τῷ ἀβρόχῳ</supplied> 

    <lb n="12"/><choice><reg>ὃ</reg><orig>το</orig></choice> μὴ εἴη τοῦ τρίτου μέρους τοῦ προει<supplied reason="lost">ρημένου</supplied> <supplied reason="lost">φόρου, ὅνπερ</supplied> 

    <lb n="13"/>φόρον νέον καθαρὸν κεκοσκι<supplied reason="lost">νευμένον</supplied> <supplied reason="lost">μέτρῳ</supplied> 

    <lb n="14"/>Ἀθηναίῳ ἀποδώσω καὶ μετρήσω <supplied reason="lost">ὑμῖν ἐν μηνὶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/>καὶ ἀποκαταστήσω τὸν αὐτὸν φόρ<unclear>ο</unclear><supplied reason="lost">ν</supplied> <supplied reason="lost">εἰς τὸν οἶκον ὑμῶν</supplied> 

    <lb n="16"/>ἐν Ἑρμουπόλει <hi rend="diaeresis">ἰ</hi>δίοις μου ζῴ<unclear>οι</unclear>ς <supplied reason="lost">καὶ ἀναλώμασιν</supplied> 

    <lb n="17"/>ἐν καιρῷ τῆς ἀποδόσεως τῶ<supplied reason="lost">ν</supplied> δη<supplied reason="lost">μοσίων</supplied> <supplied reason="lost">καὶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/>μίαν ἀξίαν ἀργυρίου ταλάντ<unclear>ων</unclear> <num value="1000"><choice><reg>χ<supplied reason="lost" cert="low">ιλίων</supplied></reg><orig><unclear>χε</unclear><supplied reason="lost" cert="low">ιλίων</supplied></orig></choice></num> <gap reason="lost" extent="unknown" unit="character"/>  

