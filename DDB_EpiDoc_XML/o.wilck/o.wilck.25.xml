    <lb n="6"/><expan>δραχ<ex>μὰς</ex></expan> <choice><reg>ὀκτώ</reg><orig>ὠγκτώ</orig></choice>, <num value="8"/> <expan><ex>γίνονται</ex></expan> <expan><ex>δραχμαὶ</ex></expan> <num value="16">ιϛ</num>. <expan>Μελάνθι<ex>ς</ex></expan> 

