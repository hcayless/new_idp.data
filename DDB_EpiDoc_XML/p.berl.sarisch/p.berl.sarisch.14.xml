    <lb n="1"/><g type="stauros"/> <app type="editorial"><lem resp="BL 11.41">δεξάμενο<supplied reason="lost">ι</supplied></lem><rdg>δεξάμενο<supplied reason="lost">ς</supplied></rdg></app> τὰ γράμματα τῆς ὑμετέρα<supplied reason="lost">ς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

