         <titleStmt>
            <title>Iliad XVIII, 23-617 (with extensive lacunae)</title>
            <author>Homer</author>
         </titleStmt>
         <publicationStmt>
            <authority>APIS</authority>
            <idno type="apisid">michigan.apis.1569</idno>
