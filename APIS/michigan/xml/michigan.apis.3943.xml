         <titleStmt>
            <title>Fragment of a Homer-Hypothesis with no gods</title>
            <author>Unknown</author>
         </titleStmt>
         <publicationStmt>
            <authority>APIS</authority>
            <idno type="apisid">michigan.apis.3943</idno>
